local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("U-HUB SUPREME | ‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢ ‚Ä¢ ‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢ ‚Ä¢ ‡∏Ñ‡∏£‡∏ö‡∏ß‡∏á‡∏à‡∏£", "DarkScene")

local Player = game.Players.LocalPlayer
local RS = game:GetService("ReplicatedStorage")
local TS = game:GetService("TweenService")
local WorldID = game.PlaceId

-- [[ üõ°Ô∏è ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏±‡∏ô‡πÅ‡∏ö‡∏ô & ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÄ‡∏ö‡∏∑‡πâ‡∏≠‡∏á‡∏ï‡πâ‡∏ô ]]
pcall(function()
    -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏±‡πâ‡∏ô‡∏Å‡∏≤‡∏£‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ö‡∏≤‡∏á‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏ó‡∏µ‡πà‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÇ‡∏î‡∏ô‡πÅ‡∏ö‡∏ô
    local g = getrawmetatable(game)
    setreadonly(g, false)
    local old = g.__namecall
    g.__namecall = newcclosure(function(self, ...)
        local method = getnamecallmethod()
        if method == "FireServer" and self.Name == "Admin_Event" then
            return nil
        end
        return old(self, ...)
    end)
end)

-- [[ ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ ]]
_G.AutoFarm = false
_G.FastAttack = false
_G.AutoStats_Melee = false
_G.AutoStats_Defense = false
_G.AutoBuyFruit = false
_G.SelectFruit = "Dough-Dough"

-- [[ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô (‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÉ‡∏´‡πâ‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢‡∏à‡∏≤‡∏Å‡πÇ‡∏î‡∏ô‡πÄ‡∏ï‡∏∞) ]]
function To(targetCFrame)
    pcall(function()
        local distance = (Player.Character.HumanoidRootPart.Position - targetCFrame.p).Magnitude
        local speed = 250 -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏µ‡πà‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢‡∏ï‡πà‡∏≠‡∏Å‡∏≤‡∏£‡πÇ‡∏î‡∏ô‡πÄ‡∏ï‡∏∞ (Kick)
        local info = TweenInfo.new(distance/speed, Enum.EasingStyle.Linear)
        local tween = TS:Create(Player.Character.HumanoidRootPart, info, {CFrame = targetCFrame})
        tween:Play()
    end)
end

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏ö‡∏∑‡πâ‡∏≠‡∏á‡∏´‡∏•‡∏±‡∏á ]]
spawn(function()
    while wait() do
        if _G.FastAttack and _G.AutoFarm then
            pcall(function() 
                RS.Remotes.Combat:FireServer() 
                game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0)) 
            end)
        end
        if _G.AutoBuyFruit then
            pcall(function() RS.Remotes.CommF_:InvokeServer("BuyFruit", _G.SelectFruit) end)
            wait(5) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡∏ã‡∏∑‡πâ‡∏≠‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
        end
    end
end)

-- [[ 1. ‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å: ‡∏ü‡∏≤‡∏£‡πå‡∏° & ‡∏™‡πÄ‡∏ï‡∏ï‡∏±‡∏™ ]]
local MainTab = Window:NewTab("‡∏ü‡∏≤‡∏£‡πå‡∏° & ‡∏™‡πÄ‡∏ï‡∏ï‡∏±‡∏™")
local FarmSection = MainTab:NewSection("üî• ‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°")
FarmSection:NewToggle("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏•‡πÄ‡∏ß‡∏•‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", "‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏≤‡∏°‡πÄ‡∏Ñ‡∏ß‡∏™", function(state) _G.AutoFarm = state end)
FarmSection:NewToggle("‡πÄ‡∏õ‡∏¥‡∏î‡∏ï‡∏µ‡πÑ‡∏ß (Fast Attack)", "‡∏ï‡∏µ‡πÄ‡∏£‡πá‡∏ß‡∏Ç‡∏∂‡πâ‡∏ô‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢", function(state) _G.FastAttack = state end)

local StatSection = MainTab:NewSection("üìä ‡∏≠‡∏±‡∏õ‡∏™‡πÄ‡∏ï‡∏ï‡∏±‡∏™")
StatSection:NewToggle("‡∏≠‡∏±‡∏õ‡∏°‡∏≤‡∏ô‡∏≤ (Melee)", "‡∏≠‡∏±‡∏õ‡πÅ‡∏ï‡πâ‡∏°‡πÉ‡∏´‡πâ‡∏´‡∏°‡∏±‡∏î‡πÅ‡∏•‡∏∞‡∏û‡∏•‡∏±‡∏á‡∏á‡∏≤‡∏ô", function(state) _G.AutoStats_Melee = state end)
StatSection:NewToggle("‡∏≠‡∏±‡∏õ‡πÄ‡∏•‡∏∑‡∏≠‡∏î (Defense)", "‡∏≠‡∏±‡∏õ‡πÅ‡∏ï‡πâ‡∏°‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏•‡∏∑‡∏≠‡∏î", function(state) _G.AutoStats_Defense = state end)

-- [[ 2. ‡∏´‡∏ô‡πâ‡∏≤‡∏ã‡∏∑‡πâ‡∏≠‡∏ú‡∏• & ‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤ ]]
local ShopTab = Window:NewTab("‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤ & ‡∏ú‡∏•‡πÑ‡∏°‡πâ")
local ShopSection = ShopTab:NewSection("üçì ‡∏£‡∏∞‡∏ö‡∏ö‡∏ã‡∏∑‡πâ‡∏≠‡∏ú‡∏•")
ShopSection:NewButton("‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π‡∏ã‡∏∑‡πâ‡∏≠‡∏ú‡∏• (‡∏î‡πà‡∏ß‡∏ô)", "‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ï‡πá‡∏≠‡∏Å‡πÑ‡∏î‡πâ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ", function() 
    Player.PlayerGui.Main.FruitShop.Visible = true 
end)
ShopSection:NewDropDown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏•‡∏ó‡∏µ‡πà‡∏à‡∏∞‡∏ã‡∏∑‡πâ‡∏≠‡∏≠‡∏≠‡πÇ‡∏ï‡πâ", "‡∏à‡∏∞‡∏ã‡∏∑‡πâ‡∏≠‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ú‡∏•‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏ï‡πá‡∏≠‡∏Å", {"Dough-Dough", "Buddha-Buddha", "Leopard-Leopard", "Dragon-Dragon"}, function(v) 
    _G.SelectFruit = v 
end)
ShopSection:NewToggle("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ã‡∏∑‡πâ‡∏≠‡∏ú‡∏•", "‡πÄ‡∏õ‡∏¥‡∏î‡∏ó‡∏¥‡πâ‡∏á‡πÑ‡∏ß‡πâ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏à‡∏∞‡πÄ‡∏≠‡∏≤‡∏ú‡∏•‡∏ô‡∏±‡πâ‡∏ô‡πÜ", function(state) _G.AutoBuyFruit = state end)

-- [[ 3. ‡∏´‡∏ô‡πâ‡∏≤‡∏†‡∏≤‡∏£‡∏Å‡∏¥‡∏à‡∏û‡∏¥‡πÄ‡∏®‡∏© (‡πÅ‡∏¢‡∏Å‡∏ï‡∏≤‡∏°‡πÇ‡∏•‡∏Å) ]]
if WorldID == 4442272160 then -- ‡πÇ‡∏•‡∏Å 2
    local RaceTab = Window:NewTab("‡∏ó‡∏≥‡πÄ‡∏ú‡πà‡∏≤ V2-V3")
    RaceTab:NewSection("üß¨ ‡∏†‡∏≤‡∏£‡∏Å‡∏¥‡∏à‡πÄ‡∏ú‡πà‡∏≤ V2 & V3")
    RaceTab:NewButton("‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™‡πÄ‡∏ú‡πà‡∏≤ V2", "‡∏Ñ‡∏∏‡∏¢‡∏Å‡∏±‡∏ö Alchemist", function() RS.Remotes.CommF_:InvokeServer("Alchemist", "1") end)
    RaceTab:NewButton("‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™‡πÄ‡∏ú‡πà‡∏≤ V3", "‡∏Ñ‡∏∏‡∏¢‡∏Å‡∏±‡∏ö Arowe", function() RS.Remotes.CommF_:InvokeServer("Arowe", "1") end)
    RaceTab:NewButton("‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏´‡∏≤‡∏î‡∏≠‡∏Å‡πÑ‡∏°‡πâ (V2)", "‡∏ö‡∏¥‡∏ô‡∏ß‡∏ô‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î‡∏î‡∏≠‡∏Å‡πÑ‡∏°‡πâ", function() To(CFrame.new(608, 15, 1533)) end)

elseif WorldID == 7449925010 then -- ‡πÇ‡∏•‡∏Å 3
    local V4Tab = Window:NewTab("‡∏ó‡∏≥‡πÄ‡∏ú‡πà‡∏≤ V4")
    local V4Section = V4Tab:NewSection("üåï ‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏ó‡∏≥ V4")
    V4Section:NewButton("‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏à‡πâ‡∏≠‡∏á‡∏û‡∏£‡∏∞‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå", "‡∏´‡∏±‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡πÑ‡∏õ‡∏´‡∏≤‡∏û‡∏£‡∏∞‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå+‡∏Å‡∏î‡∏™‡∏Å‡∏¥‡∏•", function() 
        workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.p, Vector3.new(0, 5000, 0))
        RS.Remotes.CommF_:InvokeServer("ActivateRaceV3")
    end)
    V4Section:NewButton("‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏´‡∏≤‡πÄ‡∏ü‡∏∑‡∏≠‡∏á (Blue Gear)", "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏ü‡∏∑‡∏≠‡∏á‡∏ö‡∏ô‡πÄ‡∏Å‡∏≤‡∏∞‡∏•‡∏±‡∏ö", function()
        for _, v in pairs(workspace:GetChildren()) do if v.Name == "BlueGear" then To(v.CFrame) break end end
    end)
end

-- [[ 4. ‡∏´‡∏ô‡πâ‡∏≤‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏≤‡∏á & ‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î ]]
local MiscTab = Window:NewTab("‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏≤‡∏á & ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤")
MiscTab:NewSection("üö¢ ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Ç‡πâ‡∏≤‡∏°‡πÇ‡∏•‡∏Å")
MiscTab:NewButton("‡πÑ‡∏õ‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 1", "", function() RS.Remotes.CommF_:InvokeServer("TravelMain") end)
MiscTab:NewButton("‡πÑ‡∏õ‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 2", "", function() RS.Remotes.CommF_:InvokeServer("TravelDressrosa") end)
MiscTab:NewButton("‡πÑ‡∏õ‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 3", "", function() RS.Remotes.CommF_:InvokeServer("TravelZou") end)

MiscTab:NewSection("‚öôÔ∏è ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢")
MiscTab:NewButton("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î (Anti-AFK)", "‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÄ‡∏Å‡∏°‡πÄ‡∏î‡πâ‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏¢‡∏∑‡∏ô‡πÄ‡∏â‡∏¢‡πÜ", function()
    Player.Idled:Connect(function()
        game:GetService("VirtualInputManager"):SendKeyEvent(true, "W", false, game)
        wait(0.1)
        game:GetService("VirtualInputManager"):SendKeyEvent(false, "W", false, game)
    end)
end)
