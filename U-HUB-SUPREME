-- [ SECTION 10: ADVANCED TELEPORT & WARP ]
local WarpTab = Window:AddTab({ Title = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞", Icon = "map" })

local function CreateWarp(Tab, Data, Name)
    local Sec = Tab:AddSection(Name)
    for _, v in pairs(Data) do
        Sec:AddButton({
            Title = "‡πÑ‡∏õ " .. v.Name,
            Callback = function()
                local Root = Player.Character.HumanoidRootPart
                local Distance = (Root.Position - v.Pos.p).Magnitude
                local Speed = 350
                local Tween = Services.TweenService:Create(Root, TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear), {CFrame = v.Pos})
                Tween:Play()
                Fluent:Notify({Title = "Warping", Content = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏≤‡∏á‡πÑ‡∏õ " .. v.Name})
            end
        })
    end
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÅ‡∏¢‡∏Å‡∏ï‡∏≤‡∏°‡πÇ‡∏•‡∏Å
CreateWarp(WarpTab, WorldData.World1, "‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 1 (Sea 1)")
CreateWarp(WarpTab, WorldData.World2, "‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 2 (Sea 2)")
CreateWarp(WarpTab, WorldData.World3, "‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 3 (Sea 3)")

-- [ SECTION 11: STATS AUTO-UPDATE LOGIC ]
task.spawn(function()
    while task.wait(1) do
        if Player.Data.StatsPoints.Value > 0 then
            for _, stat in pairs(StatsList) do
                if Options["Auto"..stat] and Options["Auto"..stat].Value then
                    Services.ReplicatedStorage.Remotes.CommF_:InvokeServer("AddPoint", stat, 3)
                end
            end
        end
    end
end)

-- [ SECTION 12: AUTO RECONNECT SYSTEM ]
