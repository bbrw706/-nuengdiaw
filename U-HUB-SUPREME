    Name = "Magma Village",
    QuestPos = CFrame.new(-5315.4, 12.2, 8515.2), -- ‡∏à‡∏∏‡∏î‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 1 (Military Soldier)
    Mob1 = { Name = "Military Soldier", QuestID = 1, MonsterPos = CFrame.new(-5400.2, 12.2, 8450.5) },
