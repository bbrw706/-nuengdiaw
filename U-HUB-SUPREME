-- [ SECTION 16: ADVANCED BYPASS & PERFORMANCE V2 ]
-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏ô‡∏µ‡πâ‡∏à‡∏∞‡∏ä‡πà‡∏ß‡∏¢‡πÉ‡∏´‡πâ‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ô‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏£‡πâ‡∏≠‡∏ô‡πÅ‡∏•‡∏∞‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î‡πÑ‡∏î‡πâ‡∏î‡∏µ‡∏Ç‡∏∂‡πâ‡∏ô‡∏≠‡∏µ‡∏Å
task.spawn(function()
    while task.wait(10) do
        -- ‡πÄ‡∏Ñ‡∏•‡∏µ‡∏¢‡∏£‡πå‡∏Ç‡∏¢‡∏∞‡πÉ‡∏ô‡∏´‡∏ô‡πà‡∏ß‡∏¢‡∏Ñ‡∏ß‡∏≤‡∏°‡∏à‡∏≥‡∏ó‡∏∏‡∏Å 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
        setfpscap(60) 
        for _, v in pairs(game:GetService("Lighting"):GetChildren()) do
            if v:IsA("PostProcessEffect") then v.Enabled = false end
        end
    end
end)

-- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡∏õ‡∏¥‡∏î‡∏ó‡πâ‡∏≤‡∏¢ Part 3
print("U-HUB SUPREME: Part 3 (EP3) System Online.")
Fluent:Notify({Title = "U-HUB V.MAX", Content = "‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡πâ‡∏á‡∏£‡∏∞‡∏ö‡∏ö Raid & Sea Events ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!", Duration = 5})

-- (‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡∏£‡∏ß‡∏°‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡∏à‡∏∞‡∏≠‡∏¢‡∏π‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì 750-800 ‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡πÅ‡∏•‡πâ‡∏ß‡∏Ñ‡∏£‡∏±‡∏ö‡∏ô‡πâ‡∏≤)
-- [ SECTION 17: PVP & BOUNTY HUNTER (‡∏™‡∏≤‡∏¢‡∏ö‡∏ß‡∏Å) ]
local PvPTab = Window:AddTab({ Title = "‡∏•‡πà‡∏≤‡∏Ñ‡πà‡∏≤‡∏´‡∏±‡∏ß (PvP)", Icon = "target" })

PvPTab:AddToggle("AutoBounty", {Title = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤‡∏Ñ‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (Auto Bounty)", Default = false})
PvPTab:AddToggle("AimBot", {Title = "‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô (Silent Aim)", Default = false})
PvPTab:AddDropdown("SelectTarget", {Title = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢", Values = {}, Default = "None"})

-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏Ñ‡∏ô‡πÉ‡∏ô‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü
task.spawn(function()
    while task.wait(5) do
        local pList = {}
        for _, p in pairs(Services.Players:GetPlayers()) do
            if p ~= Player then table.insert(pList, p.Name) end
        end
        if Options.SelectTarget then Options.SelectTarget:SetValues(pList) end
    end
end)

-- Logic ‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤‡∏Ñ‡πà‡∏≤‡∏´‡∏±‡∏ß
task.spawn(function()
    while task.wait() do
        if Options.AutoBounty and Options.AutoBounty.Value then
            pcall(function()
                for _, target in pairs(Services.Players:GetPlayers()) do
                    if target ~= Player and target.Character and target.Character:FindFirstChild("Humanoid") and target.Character.Humanoid.Health > 0 then
                        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏•‡∏∞‡πÇ‡∏à‡∏°‡∏ï‡∏µ
                        Player.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 5, 0)
                        VirtualUser:CaptureController()
                        VirtualUser:Button1Down(Vector2.new(1280, 672))
                        if not Options.AutoBounty.Value then break end
                    end
                end
            end)
        end
    end
end)

-- [ SECTION 18: RARE ITEMS & MASTERY (‡∏ó‡∏≥‡∏Ç‡∏≠‡∏á‡πÅ‡∏£‡∏£‡πå) ]
local ItemTab = Window:AddTab({ Title = "‡∏ó‡∏≥‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡πÅ‡∏£‡∏£‡πå", Icon = "sword" })

ItemTab:AddSection("‡∏´‡∏°‡∏±‡∏î‡πÄ‡∏ó‡∏û (Godhuman)")
ItemTab:AddToggle("FarmGodhuman", {Title = "Auto Godhuman (‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Ç‡∏≠‡∏á‡∏ó‡∏≥)", Default = false})

ItemTab:AddSection("‡∏î‡∏≤‡∏ö‡∏Ñ‡∏π‡πà (Cursed Dual Katana)")
ItemTab:AddToggle("FarmCDK", {Title = "Auto CDK (‡∏ó‡∏≥‡πÄ‡∏Ñ‡∏ß‡∏™‡πÇ‡∏•‡∏Å 3)", Default = false})

ItemTab:AddSection("‡∏Å‡∏µ‡∏ï‡∏≤‡∏£‡πå‡∏ú‡∏µ (Soul Guitar)")
ItemTab:AddToggle("FarmSoulGuitar", {Title = "Auto Soul Guitar", Default = false})

-- [ SECTION 19: DISCORD WEBHOOK LOGS ]
-- ‡∏£‡∏∞‡∏ö‡∏ö‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠‡∏ú‡πà‡∏≤‡∏ô Discord (‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡πÉ‡∏´‡πâ‡∏î‡∏π‡πÄ‡∏õ‡πá‡∏ô‡∏°‡∏∑‡∏≠‡∏≠‡∏≤‡∏ä‡∏µ‡∏û)
local function SendLog(msg)
    if _G.WebhookURL and _G.WebhookURL ~= "" then
        local data = { ["content"] = msg }
        local newdata = Services.HttpService:JSONEncode(data)
        local headers = { ["content-type"] = "application/json" }
        local request = http_request or request or HttpPost or syn.request
        request({Url = _G.WebhookURL, Method = "POST", Headers = headers, Body = newdata})
    end
end

-- [ SECTION 20: FINAL OPTIMIZER & BYPASS ]
-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡πâ‡∏≠‡∏ô‡∏Ç‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î (‡∏õ‡∏¥‡∏î‡πÅ‡∏™‡∏á‡πÄ‡∏á‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î)
Tabs.Settings:AddButton({
    Title = "‡∏•‡∏ö‡πÅ‡∏™‡∏á‡πÄ‡∏á‡∏≤ (Remove Textures / Low Graphics)",
    Callback = function()
        for _, v in pairs(game:GetDescendants()) do
            if v:IsA("BasePart") and not v:IsA("MeshPart") then
                v.Material = "SmoothPlastic"
            elseif v:IsA("Texture") or v:IsA("Decal") then
                v:Destroy()
            end
        end
        Fluent:Notify({Title = "Optimizer", Content = "‡∏•‡∏ö‡πÅ‡∏™‡∏á‡πÄ‡∏á‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡πâ‡∏≠‡∏ô‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!"})
    end
})

