RaidTab:AddToggle("AutoBuyChip", {Title = "‡∏ã‡∏∑‡πâ‡∏≠‡∏ä‡∏¥‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (1,000 Frag)", Default = false})
RaidTab:AddToggle("AutoStartRaid", {Title = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏î‡∏±‡∏ô‡πÄ‡∏à‡∏µ‡πâ‡∏¢‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", Default = false})
RaidTab:AddToggle("AutoKillRaid", {Title = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ï‡∏µ‡∏Å‡∏ß‡∏≤‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏î‡∏±‡∏ô", Default = false})

-- Logic ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏á‡∏î‡∏±‡∏ô
task.spawn(function()
    while task.wait(1) do
        pcall(function()
            if Options.AutoBuyChip and Options.AutoBuyChip.Value then
                Services.ReplicatedStorage.Remotes.CommF_:InvokeServer("RaidsNpc","Select",Options.SelectRaid.Value)
            end
            
            if Options.AutoKillRaid and Options.AutoKillRaid.Value then
                if Player.PlayerGui.Main:FindFirstChild("Timer") then
                    for _, enemy in pairs(game.Workspace.Enemies:GetChildren()) do
                        if enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
                            Player.Character.HumanoidRootPart.CFrame = enemy.HumanoidRootPart.CFrame * CFrame.new(0, 7, 0)
                        end
                    end
                end
            end
        end)
    end
end)

-- [ SECTION 14: SEA EVENTS (‡∏•‡πà‡∏≤‡πÄ‡∏à‡πâ‡∏≤‡∏ó‡∏∞‡πÄ‡∏• & ‡πÄ‡∏£‡∏∑‡∏≠‡∏ú‡∏µ) ]
local SeaTab = Window:AddTab({ Title = "‡∏•‡πà‡∏≤‡∏≠‡∏µ‡πÄ‡∏ß‡∏ô‡∏ï‡πå‡∏ó‡∏∞‡πÄ‡∏•", Icon = "waves" })

SeaTab:AddToggle("AutoSeaBeast", {Title = "‡∏•‡πà‡∏≤‡πÄ‡∏à‡πâ‡∏≤‡∏ó‡∏∞‡πÄ‡∏• (Sea Beast)", Default = false})
SeaTab:AddToggle("AutoShipRaid", {Title = "‡∏•‡πà‡∏≤‡πÄ‡∏£‡∏∑‡∏≠‡∏ú‡∏µ (Ship Raid)", Default = false})
SeaTab:AddToggle("AutoTerrorShark", {Title = "‡∏•‡πà‡∏≤‡∏â‡∏•‡∏≤‡∏°‡∏¢‡∏±‡∏Å‡∏©‡πå (Terror Shark)", Default = false})

task.spawn(function()
    while task.wait(1) do
        if Options.AutoSeaBeast and Options.AutoSeaBeast.Value then
            pcall(function()
                for _, v in pairs(game.Workspace.SeaBeasts:GetChildren()) do
                    if v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                        Player.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 150, 0)
                    end
                end
            end)
        end
    end
end)

