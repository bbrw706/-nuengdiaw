-- [[ üë§ ‡πÄ‡∏à‡πâ‡∏≤‡∏Ç‡∏≠‡∏á: ‡∏Ñ‡∏∏‡∏ì‡∏´‡∏ô‡∏∂‡πà‡∏á (Neung) ]]
-- [[ ‚å®Ô∏è ‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î‡∏´‡∏•‡∏±‡∏Å: U (‡πÄ‡∏õ‡∏¥‡∏î-‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π) ]]

-- ---------------------------------------------------------
-- [ üõ°Ô∏è SECTION 1: ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏±‡∏ô‡πÅ‡∏ö‡∏ô & ‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î ‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢ ]
-- ---------------------------------------------------------
if not game:IsLoaded() then game.Loaded:Wait() end

task.spawn(function()
    local Player = game:GetService("Players").LocalPlayer
    local MT = getrawmetatable(game)
    local OldNamecall = MT.__namecall
    setreadonly(MT, false)

    MT.__namecall = newcclosure(function(self, ...)
        local Method = getnamecallmethod()
        if Method == "Kick" or Method == "kick" then
            warn("üõ°Ô∏è U-HUB PROTECT: ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡πÇ‡∏î‡∏ô‡πÄ‡∏ï‡∏∞‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!")
            return nil
        end
        return OldNamecall(self, ...)
    end)
    setreadonly(MT, true)
end)

-- ---------------------------------------------------------
-- [ üèóÔ∏è SECTION 2: ‡πÇ‡∏´‡∏•‡∏î LIBRARY ‡πÅ‡∏•‡∏∞‡∏´‡∏ô‡πâ‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏∏‡∏õ‡∏Å‡∏£‡∏ì‡πå ]
-- ---------------------------------------------------------
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Selection = Instance.new("ScreenGui", game.CoreGui)
local Frame = Instance.new("Frame", Selection)
Frame.Size = UDim2.new(0, 350, 0, 200); Frame.Position = UDim2.new(0.5, -175, 0.5, -100); Frame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
local UICorner = Instance.new("UICorner", Frame); UICorner.CornerRadius = UDim.new(0, 15)
local UIStroke = Instance.new("UIStroke", Frame); UIStroke.Color = Color3.fromRGB(255, 0, 0); UIStroke.Thickness = 3

local Title = Instance.new("TextLabel", Frame)
Title.Size = UDim2.new(1, 0, 0, 60); Title.Text = "üíé U-HUB SUPREME üíé\n‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏° & ‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î U"; Title.TextColor3 = Color3.fromRGB(255, 255, 255); Title.TextSize = 20; Title.Font = Enum.Font.GothamBold; Title.BackgroundTransparency = 1

local MobileBtn = Instance.new("TextButton", Frame)
MobileBtn.Text = "üì± ‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠"; MobileBtn.Size = UDim2.new(0, 140, 0, 50); MobileBtn.Position = UDim2.new(0.05, 0, 0.6, 0); MobileBtn.BackgroundColor3 = Color3.fromRGB(0, 100, 255); MobileBtn.TextColor3 = Color3.fromRGB(255, 255, 255); Instance.new("UICorner", MobileBtn)

local PCBtn = Instance.new("TextButton", Frame)
PCBtn.Text = "üíª ‡∏Ñ‡∏≠‡∏°"; PCBtn.Size = UDim2.new(0, 140, 0, 50); PCBtn.Position = UDim2.new(0.53, 0, 0.6, 0); PCBtn.BackgroundColor3 = Color3.fromRGB(255, 0, 50); PCBtn.TextColor3 = Color3.fromRGB(255, 255, 255); Instance.new("UICorner", PCBtn)

-- ---------------------------------------------------------
-- [ üöÄ SECTION 3: ‡∏ü‡∏±‡∏á‡∏ä‡∏±‡πà‡∏ô‡∏´‡∏•‡∏±‡∏Å ]
-- ---------------------------------------------------------
local function RunNeungScript(platform)
    Selection:Destroy()
    local Window = Library.CreateLib("üíé U-HUB SUPREME | " .. platform, "BloodTheme")

    -- [[ ‡πÅ‡∏ó‡πá‡∏ö: ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÇ‡∏ó‡πÄ‡∏Ñ‡∏ô & ‡πÄ‡∏•‡πÄ‡∏ß‡∏• ]]
    local TabFarm = Window:NewTab("üí∞ ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏á‡∏¥‡∏ô & ‡πÄ‡∏•‡πÄ‡∏ß‡∏•")
    local SecFarm = TabFarm:NewSection("‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥")

    SecFarm:NewToggle("‡πÄ‡∏Å‡πá‡∏ö‡πÇ‡∏ó‡πÄ‡∏Ñ‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (Auto Collect)", "‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏á‡∏¥‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡∏Å‡∏ï‡∏≤‡∏°‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ", function(state)
        _G.AutoTokens = state
        task.spawn(function()
            while _G.AutoTokens do
                pcall(function()
                    for _, v in pairs(game:GetService("Workspace"):GetChildren()) do
                        if v.Name == "Token" or v:FindFirstChild("Token") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                            task.wait(0.1)
                        end
                    end
                end)
                task.wait(0.5)
            end
        end)
    end)

    SecFarm:NewToggle("‡∏ä‡∏∏‡∏ö‡πÑ‡∏ß 1 ‡∏ß‡∏¥ + ‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ä‡∏∏‡∏ö", "‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏•‡πÄ‡∏ß‡∏•‡∏à‡∏≤‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô", function(state)
        _G.FarmLevel = state
        task.spawn(function()
            while _G.FarmLevel do
                pcall(function()
                    for _, v in pairs(game.Players:GetPlayers()) do
                        if v ~= game.Players.LocalPlayer and v.Character and v.Character.Humanoid.Health <= 0 then
                            local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude
                            if dist < 20 then
                                game:GetService("ReplicatedStorage").Events.Revive:FireServer(v.Name)
                            end
                        end
                    end
                    game:GetService("ReplicatedStorage").Events.Revive:FireServer(true)
                end)
                task.wait(0.1)
            end
        end)
    end)

    -- [[ ‡πÅ‡∏ó‡πá‡∏ö: ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà (‡πÄ‡∏î‡πâ‡∏á‡∏™‡∏π‡∏á) ]]
    local TabJump = Window:NewTab("üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏î‡πâ‡∏á")
    local SecJump = TabJump:NewSection("‡πÄ‡∏î‡πâ‡∏á‡∏´‡∏ô‡∏µ‡∏ö‡∏≠‡∏ó")
    SecJump:NewDropdown("‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏î‡πâ‡∏á", "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ó‡πà‡∏≤", {"‡πÄ‡∏î‡πâ‡∏á‡∏ò‡∏£‡∏£‡∏°‡∏î‡∏≤", "‡πÄ‡∏î‡πâ‡∏á‡∏£‡∏∞‡∏î‡∏±‡∏ö‡πÄ‡∏ó‡∏û"}, function(opt) _G.LeapMode = opt end)
    SecJump:NewSlider("‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á", "‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ‡∏ï‡∏≤‡∏°‡πÉ‡∏à", 350, 50, function(s) _G.LeapPowerValue = s end)
    
    local function DoLeap()
        local hrp = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp then
            local Power = _G.LeapPowerValue or 120
            if _G.LeapMode == "‡πÄ‡∏î‡πâ‡∏á‡∏£‡∏∞‡∏î‡∏±‡∏ö‡πÄ‡∏ó‡∏û" then Power = Power * 1.6 end
            hrp.Velocity = Vector3.new(hrp.Velocity.X, Power, hrp.Velocity.Z)
        end
    end

    -- [[ ‡πÅ‡∏ó‡πá‡∏ö: ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ & ‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î ]]
    local TabSet = Window:NewTab("‚öôÔ∏è ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤")
    local SecSet = TabSet:NewSection("‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î (Keybind)")

    SecSet:NewKeybind("‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π", "‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏Ñ‡∏∑‡∏≠‡∏õ‡∏∏‡πà‡∏° U", Enum.KeyCode.U, function()
        Library:ToggleUI()
    end)

    if platform == "PC" then
        SecSet:NewKeybind("‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î‡∏Å‡∏î‡πÄ‡∏î‡πâ‡∏á‡∏™‡∏π‡∏á", "‡πÉ‡∏ä‡πâ‡πÄ‡∏°‡∏≤‡∏™‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏Ñ‡∏µ‡∏¢‡πå‡∏ö‡∏≠‡∏£‡πå‡∏î‡∏Å‡πá‡πÑ‡∏î‡πâ", Enum.KeyCode.Space, function()
            DoLeap()
        end)
    end

    -- [[ üì± ‡∏õ‡∏∏‡πà‡∏°‡∏•‡∏≠‡∏¢ N/U (Mobile Only) ]]
    if platform == "Mobile" then
        local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
        local MainBtn = Instance.new("Frame", ScreenGui)
        MainBtn.Size = UDim2.new(0, 60, 0, 60); MainBtn.Position = UDim2.new(0.05, 0, 0.4, 0)
        MainBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 20); MainBtn.Draggable = true; MainBtn.Active = true
        Instance.new("UICorner", MainBtn).CornerRadius = UDim.new(1, 0)
        local TextLabel = Instance.new("TextLabel", MainBtn)
        TextLabel.Size = UDim2.new(1, 0, 1, 0); TextLabel.Text = "N"; TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255); TextLabel.TextSize = 30; TextLabel.Font = Enum.Font.GothamBold; TextLabel.BackgroundTransparency = 1
        local ClickBtn = Instance.new("TextButton", MainBtn)
        ClickBtn.Size = UDim2.new(1, 0, 1, 0); ClickBtn.BackgroundTransparency = 1; ClickBtn.Text = ""
        ClickBtn.MouseButton1Click:Connect(function()
            Library:ToggleUI()
            TextLabel.Text = (TextLabel.Text == "N") and "U" or "N"
        end)
    end
end

-- [ üñ±Ô∏è ‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏ô‡πâ‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å ]
MobileBtn.MouseButton1Click:Connect(function() RunNeungScript("Mobile") end)
PCBtn.MouseButton1Click:Connect(function() RunNeungScript("PC") end)
