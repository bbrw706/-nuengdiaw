local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

-- [[ ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÇ‡∏•‡πÇ‡∏Å‡πâ‡∏à‡∏¥‡πã‡∏ß‡∏Ç‡πâ‡∏≤‡∏á‡∏ä‡∏∑‡πà‡∏≠ ]]
local Window = Fluent:CreateWindow({
    Title = "U-HUB SUPREME üíé", -- ‡πÄ‡∏î‡∏µ‡πã‡∏¢‡∏ß‡πÄ‡∏£‡∏≤‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡πÇ‡∏Ñ‡πâ‡∏î‡∏î‡πâ‡∏≤‡∏ô‡∏•‡πà‡∏≤‡∏á‡πÅ‡∏ó‡∏£‡∏Å‡πÇ‡∏•‡πÇ‡∏Å‡πâ‡πÄ‡∏Ç‡πâ‡∏≤‡πÑ‡∏õ
    SubTitle = "BY Neung",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false, 
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.U 
})

-- [[ ‡πÇ‡∏Ñ‡πâ‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ó‡∏£‡∏Å‡πÇ‡∏•‡πÇ‡∏Å‡πâ U-N ‡∏à‡∏¥‡πã‡∏ß‡πÑ‡∏õ‡πÑ‡∏ß‡πâ‡∏Ç‡πâ‡∏≤‡∏á‡∏ä‡∏∑‡πà‡∏≠‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå ]]
local function InsertMiniLogo()
    pcall(function()
        local TitleLabel = game.CoreGui:FindFirstChild("Fluent", true):FindFirstChild("Title", true)
        if TitleLabel and TitleLabel:IsA("TextLabel") then
            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏ü‡∏£‡∏°‡πÇ‡∏•‡πÇ‡∏Å‡πâ‡∏à‡∏¥‡πã‡∏ß
            local MiniLogo = Instance.new("Frame")
            MiniLogo.Name = "MiniLogo"
            MiniLogo.Parent = TitleLabel
            MiniLogo.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            MiniLogo.Position = UDim2.new(0, -35, 0.5, -12) -- ‡∏ß‡∏≤‡∏á‡πÑ‡∏ß‡πâ‡∏Ç‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ä‡∏∑‡πà‡∏≠
            MiniLogo.Size = UDim2.new(0, 25, 0, 25) -- ‡∏Ç‡∏ô‡∏≤‡∏î‡πÄ‡∏•‡πá‡∏Å‡∏•‡∏á‡∏ï‡∏≤‡∏°‡∏™‡∏±‡πà‡∏á
            MiniLogo.BorderSizePixel = 0
            
            local Corner = Instance.new("UICorner")
            Corner.CornerRadius = UDim.new(0, 5)
            Corner.Parent = MiniLogo
            
            local LogN = Instance.new("TextLabel")
            LogN.Parent = MiniLogo
            LogN.Size = UDim2.new(1, 0, 1, 0)
            LogN.Text = "N"
            LogN.TextColor3 = Color3.fromRGB(0, 120, 255)
            LogN.TextSize = 18
            LogN.Font = Enum.Font.GothamBold
            LogN.Rotation = 15
            LogN.BackgroundTransparency = 1
            
            local LogU = Instance.new("TextLabel")
            LogU.Parent = MiniLogo
            LogU.Size = UDim2.new(1, 0, 1, 0)
            LogU.Text = "U"
            LogU.TextColor3 = Color3.fromRGB(255, 255, 255)
            LogU.TextSize = 14
            LogU.Font = Enum.Font.GothamBold
            LogU.BackgroundTransparency = 1
        end
    end)
end
task.spawn(InsertMiniLogo)

-- [[ ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠ (‡∏ï‡∏±‡∏ß U ‡∏™‡∏µ‡∏ü‡πâ‡∏≤ + N ‡πÄ‡∏≠‡∏µ‡∏¢‡∏á‡∏´‡∏•‡∏±‡∏á) ]]
local ScreenGui = Instance.new("ScreenGui")
local MainButton = Instance.new("Frame")
ScreenGui.Name = "UHubToggle"
ScreenGui.Parent = game.CoreGui
MainButton.Parent = ScreenGui
MainButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
MainButton.Position = UDim2.new(0.1, 0, 0.15, 0)
MainButton.Size = UDim2.new(0, 60, 0, 60)
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 12)
UICorner.Parent = MainButton

local TextN = Instance.new("TextLabel")
TextN.Parent = MainButton
TextN.Text = "N"
TextN.TextColor3 = Color3.fromRGB(0, 120, 255)
TextN.TextSize = 45
TextN.Font = Enum.Font.GothamBold
TextN.Rotation = 15
TextN.BackgroundTransparency = 1
TextN.Size = UDim2.new(1, 0, 1, 0)

local TextU = Instance.new("TextLabel")
TextU.Parent = MainButton
TextU.Text = "U"
TextU.TextColor3 = Color3.fromRGB(255, 255, 255)
TextU.TextSize = 35
TextU.Font = Enum.Font.GothamBold
TextU.BackgroundTransparency = 1
TextU.Size = UDim2.new(1, 0, 1, 0)

local ClickBtn = Instance.new("TextButton")
ClickBtn.Parent = MainButton
ClickBtn.Size = UDim2.new(1, 0, 1, 0)
ClickBtn.BackgroundTransparency = 1
ClickBtn.Text = ""
ClickBtn.MouseButton1Click:Connect(function() Window:Minimize() end)

-- [ ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å ]
_G.AutoFarm = false
_G.SelectWeapon = "Melee"

-- [ Tabs ]
local Tabs = {
    Main = Window:AddTab({ Title = "General", Icon = "home" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

Tabs.Main:AddToggle("AutoFarm", {Title = "Auto Farm Level", Default = false, Callback = function(v) _G.AutoFarm = v end})

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ñ‡∏∑‡∏≠‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò & ‡∏ï‡∏µ‡∏£‡∏±‡∏ß ]]
task.spawn(function()
    while task.wait() do
        if _G.AutoFarm then
            pcall(function()
                local lp = game.Players.LocalPlayer
                local tool = lp.Backpack:FindFirstChild(_G.SelectWeapon) or lp.Character:FindFirstChild(_G.SelectWeapon)
                if tool and tool.Parent ~= lp.Character then lp.Character.Humanoid:EquipTool(tool) end
                if lp.Character:FindFirstChildOfClass("Tool") then
                    lp.Character:FindFirstChildOfClass("Tool"):Activate()
                end
            end)
        end
    end
end)

Fluent:Notify({Title = "U-HUB V.10", Content = "‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡πâ‡∏á‡πÇ‡∏•‡πÇ‡∏Å‡πâ‡∏à‡∏¥‡πã‡∏ß‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß!", Duration = 5})
