local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "U-HUB SUPREME üíé",
    SubTitle = "BY Neung",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false, -- ‡∏õ‡∏¥‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÅ‡∏™‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡∏î‡∏≠‡∏≤‡∏Å‡∏≤‡∏£‡∏ö‡∏±‡∏Ñ‡∏à‡∏≠
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.U 
})

-- [[ ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠ ]]
local ScreenGui = Instance.new("ScreenGui")
local ToggleButton = Instance.new("TextButton")
ScreenGui.Name = "UHubToggle"
ScreenGui.Parent = game.CoreGui
ToggleButton.Parent = ScreenGui
ToggleButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToggleButton.Position = UDim2.new(0.1, 0, 0.15, 0)
ToggleButton.Size = UDim2.new(0, 50, 0, 50)
ToggleButton.Text = "U"
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.TextSize = 24
local Corner = Instance.new("UICorner")
Corner.CornerRadius = UDim.new(0, 10)
Corner.Parent = ToggleButton
ToggleButton.MouseButton1Click:Connect(function() Window:Minimize() end)

-- [ ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å - ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô ]
_G.AutoFarm = false
_G.SelectWeapon = "Melee"
_G.FarmMethod = "Above (‡∏ö‡∏ô‡∏´‡∏±‡∏ß)" 
_G.FarmDistance = 10 

-- [ Tabs ]
local Tabs = {
    Main = Window:AddTab({ Title = "General", Icon = "home" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

-- [ ‡∏´‡∏ô‡πâ‡∏≤ General ]
local FarmToggle = Tabs.Main:AddToggle("AutoFarm", {Title = "Auto Farm Level", Default = false })
FarmToggle:OnChanged(function(Value)
    _G.AutoFarm = Value
end)

Tabs.Main:AddDropdown("FarmMethod", {
    Title = "‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô‡∏ï‡∏µ",
    Values = {"Above (‡∏ö‡∏ô‡∏´‡∏±‡∏ß)", "Below (‡πÉ‡∏ï‡πâ‡∏î‡∏¥‡∏ô)", "Orbit (‡∏ö‡∏¥‡∏ô‡∏ß‡∏ô)", "Fast Warp (‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏£‡∏≠‡∏ö‡∏ï‡∏±‡∏ß)"},
    Default = 1,
    Callback = function(Value) _G.FarmMethod = Value end
})

local DistanceSlider = Tabs.Main:AddSlider("Distance", {
    Title = "‡∏õ‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏¢‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á/‡∏´‡πà‡∏≤‡∏á",
    Default = 10,
    Min = 5,
    Max = 30,
    Rounding = 0,
    Callback = function(Value)
        _G.FarmDistance = Value
    end
})

-- [ ‡∏´‡∏ô‡πâ‡∏≤ Settings ]
Tabs.Settings:AddDropdown("Weapon", {
    Title = "Select Weapon",
    Values = {"Melee", "Sword", "Fruit"},
    Default = 1,
    Callback = function(Value) _G.SelectWeapon = Value end
})

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ñ‡∏∑‡∏≠‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò & ‡∏ï‡∏µ‡∏£‡∏±‡∏ß (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡πÉ‡∏´‡∏°‡πà) ]]
task.spawn(function()
    while task.wait() do
        if _G.AutoFarm then
            pcall(function()
                local lp = game.Players.LocalPlayer
                local char = lp.Character
                local tool = char:FindFirstChildOfClass("Tool") or lp.Backpack:FindFirstChild(_G.SelectWeapon)
                
                -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡∏ñ‡∏∑‡∏≠‡∏Ç‡∏≠‡∏á
                if tool and tool.Parent ~= char then
                    char.Humanoid:EquipTool(tool)
                end
                
                -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡∏ï‡∏µ (‡∏™‡πà‡∏á‡∏™‡∏±‡∏ç‡∏ç‡∏≤‡∏ì‡∏ï‡∏£‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà Tool)
                if char:FindFirstChildOfClass("Tool") then
                    game:GetService("VirtualUser"):CaptureController()
                    game:GetService("VirtualUser"):Button1Down(Vector2.new(0,0))
                    char:FindFirstChildOfClass("Tool"):Activate()
                end
            end)
        end
    end
end)

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ö‡∏¥‡∏ô‡∏ü‡∏≤‡∏£‡πå‡∏° (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ñ‡πâ‡∏≤‡∏á) ]]
task.spawn(function()
    local Angle = 0
    while task.wait() do
        if _G.AutoFarm then
            pcall(function()
                local lp = game.Players.LocalPlayer
                local char = lp.Character
                local hrp = char:FindFirstChild("HumanoidRootPart")
                
                if not lp.PlayerGui.Main.Quest.Visible then
                    hrp.CFrame = CFrame.new(1059, 15, 1549)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "BanditQuest1", 1)
                else
                    local Enemy = workspace.Enemies:FindFirstChild("Bandit")
                    if Enemy and Enemy:FindFirstChild("Humanoid") and Enemy.Humanoid.Health > 0 then
                        local EnemyHRP = Enemy.HumanoidRootPart
                        local FinalCFrame
                        
                        -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ï‡∏≤‡∏° Slider ‡πÅ‡∏•‡∏∞‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö
                        if _G.FarmMethod == "Above (‡∏ö‡∏ô‡∏´‡∏±‡∏ß)" then
                            FinalCFrame = EnemyHRP.CFrame * CFrame.new(0, _G.FarmDistance, 0)
                        elseif _G.FarmMethod == "Below (‡πÉ‡∏ï‡πâ‡∏î‡∏¥‡∏ô)" then
                            FinalCFrame = EnemyHRP.CFrame * CFrame.new(0, -_G.FarmDistance, 0)
                        elseif _G.FarmMethod == "Orbit (‡∏ö‡∏¥‡∏ô‡∏ß‡∏ô)" then
                            Angle = Angle + 0.1
                            FinalCFrame = EnemyHRP.CFrame * CFrame.Angles(0, Angle, 0) * CFrame.new(0, _G.FarmDistance, 5)
                        elseif _G.FarmMethod == "Fast Warp (‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏£‡∏≠‡∏ö‡∏ï‡∏±‡∏ß)" then
                            FinalCFrame = EnemyHRP.CFrame * CFrame.new(math.random(-5,5), _G.FarmDistance, math.random(-5,5))
                        end
                        
                        -- ‡∏ö‡∏¥‡∏ô‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏•‡∏∞‡∏´‡∏±‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏≤‡∏®‡∏±‡∏ï‡∏£‡∏π
                        hrp.CFrame = CFrame.lookAt(FinalCFrame.Position, EnemyHRP.Position)
                    else
                        hrp.CFrame = CFrame.new(1145, 20, 1630) -- ‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î‡πÅ‡∏ö‡∏ö‡∏•‡∏≠‡∏¢‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏û‡∏∑‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢‡∏Å‡∏±‡∏ô‡∏ö‡∏±‡∏Ñ
                    end
                end
            end)
        end
    end
end)

-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏±‡∏ô‡∏ï‡∏≤‡∏¢ (Anti-Fall/Void)
task.spawn(function()
    while task.wait(1) do
        if _G.AutoFarm then
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("Humanoid") then
                char.Humanoid.PlatformStand = true -- ‡∏Å‡∏±‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏•‡πâ‡∏°‡πÄ‡∏ß‡∏•‡∏≤‡∏ß‡∏≤‡∏£‡πå‡∏õ
            end
        end
    end
end)

