local Tabs = {
    Main = Window:AddTab({ Title = "Auto Farm", Icon = "rbxassetid://4483345998" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "settings" })
}

local Options = Fluent.Options

-- [[ 1. Auto Farm Tab ]]
Tabs.Main:AddParagraph({Title = "U-HUB FARMING", Content = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡∏¥‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà"})

local SelectedMob = ""
local MobDropdown = Tabs.Main:AddDropdown("MobDropdown", {
    Title = "Select Monster",
    Values = {"Monster 1", "Monster 2", "Monster 3"}, -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏≠‡∏ô‡πÉ‡∏ô‡πÄ‡∏Å‡∏°‡∏ô‡∏±‡πâ‡∏ô‡πÜ
    Multi = false,
    Default = 1,
})

MobDropdown:OnChanged(function(Value)
    SelectedMob = Value
end)

local AutoFarmToggle = Tabs.Main:AddToggle("AutoFarmToggle", {Title = "Auto Farm Level", Default = false})

-- ‡∏•‡∏≠‡∏à‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏ü‡∏≤‡∏£‡πå‡∏° (‡∏ï‡∏±‡∏ß‡∏≠‡∏¢‡πà‡∏≤‡∏á)
task.spawn(function()
    while task.wait() do
        if Options.AutoFarmToggle.Value then
            -- ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ï‡∏µ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ
            -- print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ü‡∏≤‡∏£‡πå‡∏°: " .. SelectedMob)
        end
    end
end)

-- [[ 2. Misc Tab ]]
Tabs.Misc:AddButton({
    Title = "Infinite Yield",
    Description = "‡∏£‡∏±‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÅ‡∏≠‡∏î‡∏°‡∏¥‡∏ô IY",
    Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end
})

local WalkSpeedSlider = Tabs.Misc:AddSlider("WalkSpeed", {
    Title = "WalkSpeed",
    Description = "‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡πÄ‡∏î‡∏¥‡∏ô",
    Default = 16, Min = 16, Max = 500, Rounding = 0,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

Tabs.Misc:AddButton({
    Title = "Destroy UI",
    Description = "‡∏•‡∏ö‡πÄ‡∏°‡∏ô‡∏π‡∏ó‡∏¥‡πâ‡∏á",
    Callback = function()
        Window:Destroy()
        ScreenGui:Destroy()
    end
})

Fluent:Notify({Title = "U-HUB SUPREME", Content = "‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÅ‡∏•‡∏∞‡πÅ‡∏Å‡πâ‡∏•‡∏≤‡∏Å‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô!", Duration = 5})
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 1 ]]
local Island1 = {
    Name = "Starter Island",
    Monster = "Bandit",
    QuestName = "BanditQuest1",
    QuestID = 1,
    QuestPos = CFrame.new(1059.3, 15.4, 1550.5), -- ‡∏à‡∏∏‡∏î‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™
    MonsterPos = CFrame.new(1185.2, 7.3, 1620.4)  -- ‡∏à‡∏∏‡∏î‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå
}

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏ô UI ]]
Tabs.Main:AddParagraph({Title = "ISLAND 1", Content = "‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏Å‡∏≤‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 0-10)"})

Tabs.Main:AddToggle("FarmIsland1", {
    Title = "Auto Farm: " .. Island1.Name,
    Default = false
}):OnChanged(function(Value)
    _G.AutoFarm_Island1 = Value
end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 1 ]]
task.spawn(function()
    while task.wait() do
        if _G.AutoFarm_Island1 then
            pcall(function()
                local MyChar = game.Players.LocalPlayer.Character
                local Root = MyChar.HumanoidRootPart
                
                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á (‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÉ‡∏´‡πâ‡∏´‡∏¢‡∏¥‡∏ö)
                if not MyChar:FindFirstChildOfClass("Tool") then
                    for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                        if tool:IsA("Tool") then
                            MyChar.Humanoid:EquipTool(tool)
                        end
                    end
                end

                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡∏°‡∏µ‡πÄ‡∏Ñ‡∏ß‡∏™‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á
                if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("Quest") then
                    -- üìç ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™‡∏î‡πâ‡∏ß‡∏¢‡∏û‡∏¥‡∏Å‡∏±‡∏î
                    Root.CFrame = Island1.QuestPos
                    task.wait(0.5)
                    -- ‡∏™‡πà‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™‡πÑ‡∏õ‡∏ó‡∏µ‡πà Server
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", Island1.QuestName, Island1.QuestID)
                else
                    -- üìç ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡πÄ‡∏Ñ‡∏ß‡∏™‡πÅ‡∏•‡πâ‡∏ß ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏à‡∏∏‡∏î‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå
                    local Target = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == Island1.Monster and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            Target = v
                            break
                        end
                    end

                    if Target then
                        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏≠‡∏¢‡∏π‡πà‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå (‡∏£‡∏∞‡∏¢‡∏∞‡∏´‡πà‡∏≤‡∏á 10 ‡∏´‡∏ô‡πà‡∏ß‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢)
                        Root.CFrame = Target.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)
                        
                        -- ‡∏™‡πà‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÇ‡∏à‡∏°‡∏ï‡∏µ
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Attack", {
                            [1] = Target.HumanoidRootPart.Position,
                            [2] = {[1] = Target}
                        })
                    else
                        -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏Å‡∏¥‡∏î ‡πÉ‡∏´‡πâ‡πÑ‡∏õ‡∏£‡∏≠‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏°‡∏≠‡∏ô‡πÄ‡∏Å‡∏¥‡∏î
                        Root.CFrame = Island1.MonsterPos
                    end
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 2 ]]
local Island2 = {
    Name = "Jungle",
    QuestPos = CFrame.new(-1598.5, 35.5, 153.2), -- ‡∏à‡∏∏‡∏î‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™ (‡∏•‡∏¥‡∏á/‡∏Å‡∏≠‡∏£‡∏¥‡∏•‡∏•‡πà‡∏≤ ‡∏£‡∏±‡∏ö‡∏ó‡∏µ‡πà‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô)
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 1
    Mob1 = {
        Name = "Monkey",
        QuestID = 1,
        MonsterPos = CFrame.new(-1623.1, 15.6, 6.7)
    },
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 2
    Mob2 = {
        Name = "Gorilla",
        QuestID = 2,
        MonsterPos = CFrame.new(-1204.6, 5.6, -500.8)
    }
}

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏ô UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 2 ]]
Tabs.Main:AddParagraph({Title = "ISLAND 2", Content = "‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏Å‡∏≤‡∏∞‡∏•‡∏¥‡∏á (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 10-30)"})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏•‡∏¥‡∏á
Tabs.Main:AddToggle("FarmMonkey", {
    Title = "Auto Farm: Monkey",
    Default = false
}):OnChanged(function(Value)
    _G.AutoFarm_Monkey = Value
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏≠‡∏£‡∏¥‡∏•‡∏•‡πà‡∏≤
Tabs.Main:AddToggle("FarmGorilla", {
    Title = "Auto Farm: Gorilla",
    Default = false
}):OnChanged(function(Value)
    _G.AutoFarm_Gorilla = Value
end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 2 ]]
task.spawn(function()
    while task.wait() do
        if _G.AutoFarm_Monkey or _G.AutoFarm_Gorilla then
            pcall(function()
                local MyChar = game.Players.LocalPlayer.Character
                local Root = MyChar.HumanoidRootPart
                
                -- ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏≤‡∏°‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡∏¥‡∏î
                local TargetData = _G.AutoFarm_Monkey and Island2.Mob1 or Island2.Mob2
                
                -- 1. ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏Ñ‡∏ß‡∏™
                if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("Quest") then
                    Root.CFrame = Island2.QuestPos
                    task.wait(0.5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "JungleQuest", TargetData.QuestID)
                else
                    -- 2. ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ï‡∏µ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå
                    local Enemy = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == TargetData.Name and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            Enemy = v
                            break
                        end
                    end

                    if Enemy then
                        Root.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)
                        -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Attack", {
                            [1] = Enemy.HumanoidRootPart.Position,
                            [2] = {[1] = Enemy}
                        })
                    else
                        -- ‡∏£‡∏≠‡∏°‡∏≠‡∏ô‡πÄ‡∏Å‡∏¥‡∏î
                        Root.CFrame = TargetData.MonsterPos
                    end
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 3 ]]
local Island3 = {
    Name = "Pirate Village",
    QuestPos = CFrame.new(-1140.2, 4.8, 3828.3), -- ‡∏à‡∏∏‡∏î‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 1 (Pirate)
    Mob1 = { Name = "Pirate", QuestID = 1, MonsterPos = CFrame.new(-1218.4, 4.8, 3915.2) },
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 2 (Brute)
    Mob2 = { Name = "Brute", QuestID = 2, MonsterPos = CFrame.new(-1352.4, 15.3, 4292.1) },
    -- ‡∏ö‡∏≠‡∏™ (The Chop / Buggy)
    Boss = { Name = "The Chop", QuestID = 3, MonsterPos = CFrame.new(-1150.2, 4.8, 3850.5) }
}

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏ô UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 3 ]]
Tabs.Main:AddParagraph({Title = "ISLAND 3", Content = "‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏Å‡∏≤‡∏∞‡∏ö‡∏±‡∏Å‡∏Å‡∏µ‡πâ (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 30-60)"})

Tabs.Main:AddToggle("FarmPirate", { Title = "Auto Farm: Pirate", Default = false }):OnChanged(function(v) _G.Farm_Pirate = v end)
Tabs.Main:AddToggle("FarmBrute", { Title = "Auto Farm: Brute", Default = false }):OnChanged(function(v) _G.Farm_Brute = v end)
Tabs.Main:AddToggle("FarmBuggy", { Title = "Auto Farm: Buggy (Boss)", Default = false }):OnChanged(function(v) _G.Farm_Buggy = v end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 3 ]]
task.spawn(function()
    while task.wait() do
        if _G.Farm_Pirate or _G.Farm_Brute or _G.Farm_Buggy then
            pcall(function()
                local MyChar = game.Players.LocalPlayer.Character
                local Root = MyChar.HumanoidRootPart
                
                -- ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ï‡∏≤‡∏°‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡∏¥‡∏î
                local TargetData = nil
                if _G.Farm_Pirate then TargetData = Island3.Mob1
                elseif _G.Farm_Brute then TargetData = Island3.Mob2
                elseif _G.Farm_Buggy then TargetData = Island3.Boss end
                
                if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("Quest") then
                    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™
                    Root.CFrame = Island3.QuestPos
                    task.wait(0.5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "BuggyQuest1", TargetData.QuestID)
                else
                    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ï‡∏µ
                    local Enemy = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == TargetData.Name and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            Enemy = v
                            break
                        end
                    end

                    if Enemy then
                        Root.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Attack", {[1] = Enemy.HumanoidRootPart.Position, [2] = {[1] = Enemy}})
                    else
                        Root.CFrame = TargetData.MonsterPos
                    end
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 4 ]]
