-- [ SECTION 1: MASTER BOOTLOADER & SECURITY ]
if not game:IsLoaded() then game.Loaded:Wait() end
local Services = setmetatable({}, {__index = function(t, k) return game:GetService(k) end})
local Player = Services.Players.LocalPlayer
local LogService = Services.LogService
local HttpService = Services.HttpService
local RunService = Services.RunService
local VirtualUser = Services.VirtualUser

-- Anti-Kick & Security Bypass (‡∏´‡πâ‡∏≤‡∏°‡∏•‡∏ö)
local hook; hook = hookmetamethod(game, "__namecall", function(self, ...)
    local method = getnamecallmethod()
    if not checkcaller() and (method == "Kick" or method == "kick") then 
        return nil 
    end
    return hook(self, ...)
end)

-- [ SECTION 2: ADVANCED OPTIMIZATION (‡∏£‡∏∞‡∏ö‡∏ö‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ RAM) ]
task.spawn(function()
    while task.wait(10) do
        collectgarbage("collect")
        setfpscap(60)
        for i,v in pairs(getconnections(LogService.MessageOut)) do v:Disable() end
    end
end)

-- [ SECTION 3: PREMIUM UI INITIALIZATION ]
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local Window = Fluent:CreateWindow({
    Title = "U-HUB üíé V.MAX [PREMIUM]",
    SubTitle = "Owner: Na-Nueng (V.MAX Edition)",
    TabWidth = 160, 
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, 
    Theme = "Darker", 
    MinimizeKey = Enum.KeyCode.U 
})

-- [[ üîò MOBILE TOGGLE BUTTON (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î-‡πÄ‡∏õ‡∏¥‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ô‡πâ‡∏≤‡∏´‡∏ô‡∏∂‡πà‡∏á) ]]
local ToggleBtn = Instance.new("ScreenGui")
local MainBtn = Instance.new("TextButton")
local Corner = Instance.new("UICorner")

ToggleBtn.Name = "UHubPro"
ToggleBtn.Parent = (game:GetService("CoreGui") or Player.PlayerGui)
MainBtn.Name = "Main"
MainBtn.Parent = ToggleBtn
MainBtn.BackgroundColor3 = Color3.fromRGB(120, 0, 200)
MainBtn.Position = UDim2.new(0.05, 0, 0.15, 0)
MainBtn.Size = UDim2.new(0, 55, 0, 55)
MainBtn.Text = "U"
MainBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
MainBtn.TextSize = 28
MainBtn.Draggable = true
Corner.CornerRadius = UDim.new(0, 50)
Corner.Parent = MainBtn

MainBtn.MouseButton1Click:Connect(function()
    if game:GetService("CoreGui"):FindFirstChild("Fluent") then
        game:GetService("CoreGui").Fluent.Enabled = not game:GetService("CoreGui").Fluent.Enabled
    end
end)

-- [ SECTION 4: TABS & CATEGORIES ]
local Tabs = {
    Main = Window:AddTab({ Title = "‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏´‡∏•‡∏±‡∏Å", Icon = "home" }),
    Boss = Window:AddTab({ Title = "‡∏•‡πà‡∏≤‡∏ö‡∏≠‡∏™", Icon = "skull" }),
    Warp = Window:AddTab({ Title = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞", Icon = "map" }),
    Raid = Window:AddTab({ Title = "‡∏•‡∏á‡∏î‡∏±‡∏ô‡πÄ‡∏à‡∏µ‡πâ‡∏¢‡∏ô", Icon = "activity" }),
    PvP = Window:AddTab({ Title = "‡∏•‡πà‡∏≤‡∏Ñ‡πà‡∏≤‡∏´‡∏±‡∏ß", Icon = "target" }),
    Sea = Window:AddTab({ Title = "‡∏•‡πà‡∏≤‡∏≠‡∏µ‡πÄ‡∏ß‡∏ô‡∏ï‡πå‡∏ó‡∏∞‡πÄ‡∏•", Icon = "waves" }),
    Item = Window:AddTab({ Title = "‡∏ó‡∏≥‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡πÅ‡∏£‡∏£‡πå", Icon = "sword" }),
    Stats = Window:AddTab({ Title = "‡∏≠‡∏±‡∏õ‡∏™‡πÄ‡∏ï‡∏ï‡∏±‡∏™", Icon = "bar-chart" }),
    Settings = Window:AddTab({ Title = "‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏£‡∏∞‡∏ö‡∏ö", Icon = "settings" })
}
local Options = Fluent.Options

-- [ SECTION 5: AUTO FARM LOGIC ]
Tabs.Main:AddToggle("AutoFarm", {Title = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏•‡πÄ‡∏ß‡∏•‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", Default = false})
Tabs.Main:AddToggle("FastAttack", {Title = "‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏™‡∏á (Fast Attack)", Default = true})
Tabs.Main:AddDropdown("Weapon", {Title = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò", Values = {"Melee", "Sword", "Fruit"}, Default = "Melee"})

task.spawn(function()
    while task.wait() do
        pcall(function()
            if Options.AutoFarm.Value then
                local tool = Player.Backpack:FindFirstChild(Options.Weapon.Value) or Player.Character:FindFirstChild(Options.Weapon.Value)
                if tool then Player.Character.Humanoid:EquipTool(tool) end
                
                if Options.FastAttack.Value then
                    VirtualUser:CaptureController()
                    VirtualUser:Button1Down(Vector2.new(1280, 672))
                end
                
                for _, enemy in pairs(game.Workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
                        Player.Character.HumanoidRootPart.CFrame = enemy.HumanoidRootPart.CFrame * CFrame.new(0, 8, 0)
                        break
                    end
                end
            end
        end)
    end
end)

-- [ SECTION 6: WORLD DATABASE (‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏Ñ‡∏£‡∏ö 3 ‡πÇ‡∏•‡∏Å) ]
local WorldData = {
    ["Sea1"] = {
        {Name = "Bandit [Lv. 0]", Pos = CFrame.new(1059, 15, 1545)},
        {Name = "Monkey [Lv. 10]", Pos = CFrame.new(-1598, 35, 153)},
        {Name = "Gorilla [Lv. 15]", Pos = CFrame.new(-1200, 50, -500)},
        {Name = "Pirate [Lv. 35]", Pos = CFrame.new(-1146, 4, 3827)}
    },
    ["Sea2"] = {
        {Name = "Raider [Lv. 700]", Pos = CFrame.new(-424, 72, 1836)},
        {Name = "Zombie [Lv. 925]", Pos = CFrame.new(-5622, 15, -737)}
    },
    ["Sea3"] = {
        {Name = "Reborn Skeleton [Lv. 1975]", Pos = CFrame.new(-9500, 160, 5700)},
        {Name = "Peanut Scout [Lv. 2075]", Pos = CFrame.new(-13200, 510, -8100)}
    }
}

local function CreateWarp(Tab, Data, Name)
    local Sec = Tab:AddSection(Name)
    for _, v in pairs(Data) do
        Sec:AddButton({
            Title = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ " .. v.Name,
            Callback = function()
                Services.TweenService:Create(Player.Character.HumanoidRootPart, TweenInfo.new(2), {CFrame = v.Pos}):Play()
            end
        })
    end
end
CreateWarp(Tabs.Warp, WorldData.Sea1, "‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 1 (Sea 1)")
CreateWarp(Tabs.Warp, WorldData.Sea2, "‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 2 (Sea 2)")
CreateWarp(Tabs.Warp, WorldData.Sea3, "‡πÇ‡∏•‡∏Å‡∏ó‡∏µ‡πà 3 (Sea 3)")

-- [ SECTION 7: RAID & SEA EVENTS ]
Tabs.Raid:AddToggle("AutoBuyChip", {Title = "‡∏ã‡∏∑‡πâ‡∏≠‡∏ä‡∏¥‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", Default = false})
Tabs.Sea:AddToggle("AutoSeaBeast", {Title = "‡∏•‡πà‡∏≤‡πÄ‡∏à‡πâ‡∏≤‡∏ó‡∏∞‡πÄ‡∏• (Sea Beast)", Default = false})

-- [ SECTION 8: AUTO STATS ]
local StatsList = {"Melee", "Defense", "Sword", "Gun", "Blox Fruit"}
for _, stat in pairs(StatsList) do
    Tabs.Stats:AddToggle("Auto"..stat, {Title = "‡∏≠‡∏±‡∏õ "..stat, Default = false})
end

task.spawn(function()
    while task.wait(1) do
        if Player.Data.StatsPoints.Value > 0 then
            for _, stat in pairs(StatsList) do
                if Options["Auto"..stat] and Options["Auto"..stat].Value then
                    Services.ReplicatedStorage.Remotes.CommF_:InvokeServer("AddPoint", stat, 3)
                end
            end
        end
    end
end)

-- [ SECTION 9: SETTINGS & PREMIUM LANGUAGE ]
