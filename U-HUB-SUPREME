local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

-- ‡πÅ‡∏Å‡πâ‡∏à‡∏∏‡∏î‡∏ï‡∏≤‡∏¢: ‡πÉ‡∏ä‡πâ‡∏ä‡∏∑‡πà‡∏≠ Theme "Midnight" (‡∏´‡πâ‡∏≤‡∏°‡πÉ‡∏™‡πà‡∏™‡∏µ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡πÄ‡∏≠‡∏á‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏à‡∏∞‡∏ó‡∏≥‡πÉ‡∏´‡πâ Error ‡πÅ‡∏ö‡∏ö‡πÉ‡∏ô‡∏£‡∏π‡∏õ)
local Window = Library.CreateLib("U-HUB SUPREME üíé", "Midnight")

-- [ Tab: Main ]
local MainTab = Window:NewTab("‡∏ü‡∏≤‡∏£‡πå‡∏° & ‡∏ö‡∏≠‡∏™")
local MainSection = MainTab:NewSection("‡∏£‡∏∞‡∏ö‡∏ö‡∏´‡∏•‡∏±‡∏Å")

_G.AutoFarm = false
_G.FastAttack = false

MainSection:NewToggle("‡πÄ‡∏õ‡∏¥‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏•‡πÄ‡∏ß‡∏•", "‡∏ï‡∏µ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå", function(v)
    _G.AutoFarm = v
end)

MainSection:NewToggle("‡∏ï‡∏µ‡πÄ‡∏£‡πá‡∏ß (Fast Attack)", "‡∏•‡∏î‡∏≠‡∏≤‡∏Å‡∏≤‡∏£‡∏´‡∏ô‡πà‡∏ß‡∏á‡∏Ç‡∏≠‡∏á‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á", function(v)
    _G.FastAttack = v
end)

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô: ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡∏≠‡∏≤‡∏Å‡∏≤‡∏£‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å ]]
task.spawn(function()
    while true do
        -- ‡πÉ‡∏ä‡πâ task.wait(0.1) ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ Log DMG ‡πÄ‡∏î‡πâ‡∏á‡∏£‡∏±‡∏ß‡∏à‡∏ô‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á Acer ‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å‡∏Ñ‡πâ‡∏≤‡∏á
        task.wait(0.1) 
        if _G.AutoFarm then
            pcall(function()
                -- ‡∏™‡∏±‡πà‡∏á‡πÇ‡∏à‡∏°‡∏ï‡∏µ
                game:GetService("ReplicatedStorage").Remotes.Combat:FireServer()
                game:GetService("VirtualUser"):Button1Down(Vector2.new(0,0))
            end)
        end
    end
end)

-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° U ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏¥‡∏î/‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π
game:GetService("UserInputService").InputBegan:Connect(function(input, processed)
    if not processed and input.KeyCode == Enum.KeyCode.U then
        Library:toggleGui()
    end
end)

print("U-HUB: ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß!")
