Tabs.Main:AddToggle("FarmVampire", { Title = "Auto Farm: Vampire", Default = false }):OnChanged(function(v) _G.Farm_Vampire = v end)

Tabs.Main:AddParagraph({Title = "SNOW MOUNTAIN", Content = "‡∏†‡∏π‡πÄ‡∏Ç‡∏≤‡∏´‡∏¥‡∏°‡∏∞ (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 1000-1100)"})
Tabs.Main:AddToggle("FarmSnowTrooper", { Title = "Auto Farm: Snow Trooper", Default = false }):OnChanged(function(v) _G.Farm_SnowTrooper = v end)
Tabs.Main:AddToggle("FarmWinterWarrior", { Title = "Auto Farm: Winter Warrior", Default = false }):OnChanged(function(v) _G.Farm_WinterWarrior = v end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏Å‡∏±‡∏î ]]
task.spawn(function()
    while task.wait() do
        if _G.Farm_Zombie or _G.Farm_Vampire or _G.Farm_SnowTrooper or _G.Farm_WinterWarrior then
            pcall(function()
                local MyChar = game.Players.LocalPlayer.Character
                local Root = MyChar.HumanoidRootPart
                
                local Target, QName, QID, QPos, MPos = nil, "", 1, nil, nil

                if _G.Farm_Zombie then
                    Target, QName, QID = Sea2_Late_Data.Graveyard.Mob1.Name, "ZombieQuest", 1
                    QPos, MPos = Sea2_Late_Data.Graveyard.QuestPos, Sea2_Late_Data.Graveyard.Mob1.MonsterPos
                elseif _G.Farm_Vampire then
                    Target, QName, QID = Sea2_Late_Data.Graveyard.Mob2.Name, "ZombieQuest", 2
                    QPos, MPos = Sea2_Late_Data.Graveyard.QuestPos, Sea2_Late_Data.Graveyard.Mob2.MonsterPos
                elseif _G.Farm_SnowTrooper then
                    Target, QName, QID = Sea2_Late_Data.SnowMountain.Mob1.Name, "SnowMountainQuest", 1
                    QPos, MPos = Sea2_Late_Data.SnowMountain.QuestPos, Sea2_Late_Data.SnowMountain.Mob1.MonsterPos
                elseif _G.Farm_WinterWarrior then
                    Target, QName, QID = Sea2_Late_Data.SnowMountain.Mob2.Name, "SnowMountainQuest", 2
                    QPos, MPos = Sea2_Late_Data.SnowMountain.QuestPos, Sea2_Late_Data.SnowMountain.Mob2.MonsterPos
                end

                if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("Quest") then
                    Root.CFrame = QPos
                    task.wait(0.5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", QName, QID)
                else
                    local Enemy = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == Target and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            Enemy = v
                            break
                        end
                    end
                    if Enemy then
                        Root.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0, 12, 0)
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Attack", {[1] = Enemy.HumanoidRootPart.Position, [2] = {[1] = Enemy}})
                    else
                        Root.CFrame = MPos
                    end
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞ Hot and Cold ]]
