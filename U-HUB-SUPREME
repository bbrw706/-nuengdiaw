- [[ U-HUB SUPREME | ULTIMATE EDITION ]]
-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏¥‡∏î-‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π‡∏î‡πâ‡∏ß‡∏¢‡∏õ‡∏∏‡πà‡∏°: [ U ]

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("U-HUB SUPREME üíé (v2026)", "DarkScene")

-- [ ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î-‡πÄ‡∏õ‡∏¥‡∏î ]
_G.ToggleKey = Enum.KeyCode.U -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏õ‡∏∏‡πà‡∏° U

-- ‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£
_G.AutoFarm = false
_G.FastAttack = false
_G.AutoBoss = false
_G.SelectedBoss = ""
_G.SelectWeapon = "Melee"

local BossList = {"Saber Expert", "The Saw", "Greybeard", "Don Swan", "Jeremy", "Fajita", "Ice Admiral", "Tide Keeper"}

-- [ Tab: ‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å ]
local MainTab = Window:NewTab("‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å")
local MainSection = MainTab:NewSection("‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°")

MainSection:NewDropdown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò", "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°", {"Melee", "Sword", "Fruit"}, function(v)
    _G.SelectWeapon = v
end)

MainSection:NewToggle("‡πÄ‡∏õ‡∏¥‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏•‡πÄ‡∏ß‡∏•", "‡∏ï‡∏µ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", function(v) _G.AutoFarm = v end)
MainSection:NewToggle("‡∏ï‡∏µ‡πÄ‡∏£‡πá‡∏ß (Fast Attack)", "", function(v) _G.FastAttack = v end)

-- [ Tab: ‡∏£‡∏∞‡∏ö‡∏ö‡∏ö‡∏≠‡∏™ ]
local BossTab = Window:NewTab("‡∏•‡πà‡∏≤‡∏ö‡∏≠‡∏™")
local BossSection = BossTab:NewSection("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ö‡∏≠‡∏™")

BossSection:NewDropdown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ö‡∏≠‡∏™", "", BossList, function(v) _G.SelectedBoss = v end)
BossSection:NewToggle("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡∏µ‡∏ö‡∏≠‡∏™ (Auto Boss)", "", function(v) _G.AutoBoss = v end)

-- [ Tab: ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ ]
local ConfigTab = Window:NewTab("‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤")
local ConfigSection = ConfigTab:NewSection("‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î-‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π")

ConfigSection:NewKeybind("‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î-‡πÄ‡∏õ‡∏¥‡∏î", "‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô", _G.ToggleKey, function(key)
	Library:toggleGui() -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡∏≠‡∏á Kavo ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏• GUI
end)

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏õ‡∏∏‡πà‡∏°‡∏Å‡∏î (UserInputService) ]]
game:GetService("UserInputService").InputBegan:Connect(function(input, processed)
    if not processed and input.KeyCode == _G.ToggleKey then
        Library:toggleGui()
    end
end)

-- [ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏ß‡∏°‡πÉ‡∏™‡πà‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò ]
function EquipWeapon()
    pcall(function()
        for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.ToolTip == _G.SelectWeapon then
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
            end
        end
    end)
end

-- [ ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏Å ]
spawn(function()
    while task.wait() do
        if _G.AutoFarm or _G.AutoBoss then
            pcall(function()
                EquipWeapon()
                
                if _G.AutoBoss and _G.SelectedBoss ~= "" then
                    local Boss = workspace.Enemies:FindFirstChild(_G.SelectedBoss)
                    if Boss and Boss:FindFirstChild("Humanoid") and Boss.Humanoid.Health > 0 then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Boss.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)
                        if _G.FastAttack then
                            game:GetService("ReplicatedStorage").Remotes.Combat:FireServer()
                            game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0))
                        end
                    end
                elseif _G.AutoFarm then
                    if _G.FastAttack then
                        game:GetService("ReplicatedStorage").Remotes.Combat:FireServer()
                        game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0))
                    end
                end
            end)
        end
    end
end)

print("U-HUB ULTIMATE: ‡∏Å‡∏î [ U ] ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏¥‡∏î/‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π")
