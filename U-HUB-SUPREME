local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "U-HUB SUPREME üíé BY(Nueng)",
    SubTitle = "Mobile & PC Premium",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, 
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.U -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏õ‡∏∏‡πà‡∏° U ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏¢‡πà‡∏≠‡πÄ‡∏°‡∏ô‡∏π‡πÉ‡∏ô‡∏£‡∏∞‡∏ö‡∏ö Fluent
})

-- [ ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å ]
_G.AutoFarm = false
_G.FastAttack = false
_G.SelectWeapon = "Melee"

-- [ ‡∏™‡∏£‡πâ‡∏≤‡∏á Tabs ]
local Tabs = {
    Main = Window:AddTab({ Title = "General", Icon = "home" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

-- [ ‡∏´‡∏ô‡πâ‡∏≤ General ]
Tabs.Main:AddParagraph({
    Title = "Auto Farm Level",
    Content = "‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡πÉ‡∏ô‡∏´‡∏ô‡πâ‡∏≤ Settings ‡∏Å‡πà‡∏≠‡∏ô)"
})

local FarmToggle = Tabs.Main:AddToggle("AutoFarm", {Title = "Auto Farm Level", Default = false })
FarmToggle:OnChanged(function(Value)
    _G.AutoFarm = Value
end)

local FastToggle = Tabs.Main:AddToggle("FastAttack", {Title = "Fast Attack (‡∏ï‡∏µ‡∏£‡∏±‡∏ß‡∏°‡∏≤‡∏Å‡πÜ)", Default = false })
FastToggle:OnChanged(function(Value)
    _G.FastAttack = Value
end)

-- [ ‡∏´‡∏ô‡πâ‡∏≤ Settings ]
local Dropdown = Tabs.Settings:AddDropdown("Weapon", {
    Title = "Select Combat / Weapon",
    Values = {"Melee", "Sword", "Fruit"},
    Multi = false,
    Default = 1,
})

Dropdown:OnChanged(function(Value)
    _G.SelectWeapon = Value
end)

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡πÅ‡∏Å‡πâ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° U ‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î (Force Toggle) ]]
local UserInputService = game:GetService("UserInputService")
UserInputService.InputBegan:Connect(function(input, processed)
    -- ‡∏î‡∏±‡∏Å‡∏à‡∏±‡∏ö‡∏õ‡∏∏‡πà‡∏° U ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á (Raw Key Input) ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏ä‡πâ ‡∏õ‡∏¥‡∏î/‡πÄ‡∏õ‡∏¥‡∏î ‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á
    if input.KeyCode == Enum.KeyCode.U then
        Window:Minimize() 
    end
end)

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ï‡∏µ‡∏£‡∏±‡∏ß (Fast Attack) - ‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÉ‡∏´‡∏°‡πà ]]
task.spawn(function()
    while true do
        task.wait() -- ‡∏£‡∏±‡∏ô‡∏î‡πâ‡∏ß‡∏¢‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î
        if _G.FastAttack and _G.AutoFarm then
            pcall(function()
                local Combat = game:GetService("ReplicatedStorage").Remotes.Combat
                -- ‡∏™‡πà‡∏á‡∏™‡∏±‡∏ç‡∏ç‡∏≤‡∏ì‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÅ‡∏ö‡∏ö‡∏£‡∏±‡∏ß‡∏û‡∏¥‡πÄ‡∏®‡∏© (Multi-Fire)
                Combat:FireServer()
                Combat:FireServer()
                Combat:FireServer()
                Combat:FireServer()
            end)
        end
    end
end)

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ö‡∏¥‡∏ô‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÅ‡∏•‡∏∞‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™ ]]
task.spawn(function()
    while task.wait(0.1) do
        if _G.AutoFarm then
            pcall(function()
                local char = game.Players.LocalPlayer.Character
                -- ‡∏£‡∏∞‡∏ö‡∏ö‡∏´‡∏¢‡∏¥‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
                if char and not char:FindFirstChildOfClass("Tool") then
                    local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(_G.SelectWeapon)
                    if tool then char.Humanoid:EquipTool(tool) end
                end

                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏Ñ‡∏ß‡∏™‡πÅ‡∏•‡∏∞‡∏ö‡∏¥‡∏ô‡πÑ‡∏õ‡∏ü‡∏≤‡∏£‡πå‡∏°
                if not game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible then
                    char.HumanoidRootPart.CFrame = CFrame.new(1059, 15, 1549) -- NPC ‡πÄ‡∏Å‡∏≤‡∏∞‡πÅ‡∏£‡∏Å
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "BanditQuest1", 1)
                else
                    local Enemy = workspace.Enemies:FindFirstChild("Bandit")
                    if Enemy and Enemy:FindFirstChild("Humanoid") and Enemy.Humanoid.Health > 0 then
                        char.HumanoidRootPart.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)
                    else
                        char.HumanoidRootPart.CFrame = CFrame.new(1145, 15, 1630) -- ‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î
                    end
                end
            end)
        end
    end
end)

Fluent:Notify({
    Title = "U-HUB Ready!",
    Content = "UI ‡∏™‡πÑ‡∏ï‡∏•‡πå Maru ‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß | ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° [ U ] ‡πÄ‡∏û‡∏∑‡πà‡∏≠ ‡∏õ‡∏¥‡∏î/‡πÄ‡∏õ‡∏¥‡∏î ‡πÄ‡∏°‡∏ô‡∏π",
    Duration = 5
})
