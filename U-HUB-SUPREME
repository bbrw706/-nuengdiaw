    RunService:Set3dRenderingEnabled(not Options.WhiteScreen.Value) 
end)

local CreditSec = Tabs.Settings:AddSection("‡∏•‡∏¥‡∏Ç‡∏™‡∏¥‡∏ó‡∏ò‡∏¥‡πå‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå")
CreditSec:AddParagraph({
    Title = "‡πÄ‡∏à‡πâ‡∏≤‡∏Ç‡∏≠‡∏á‡πÇ‡∏õ‡∏£‡πÄ‡∏à‡∏Å‡∏ï‡πå: ‡∏ô‡πâ‡∏≤‡∏´‡∏ô‡∏∂‡πà‡∏á (Na-Nueng)",
