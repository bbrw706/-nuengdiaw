local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "U-HUB SUPREME üíé",
    SubTitle = "BY Neung",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, 
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.U 
})

-- [[ ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠ ]]
local ScreenGui = Instance.new("ScreenGui")
local ToggleButton = Instance.new("TextButton")
ScreenGui.Name = "UHubToggle"
ScreenGui.Parent = game.CoreGui
ToggleButton.Parent = ScreenGui
ToggleButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToggleButton.Position = UDim2.new(0.1, 0, 0.15, 0)
ToggleButton.Size = UDim2.new(0, 50, 0, 50)
ToggleButton.Text = "U"
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.TextSize = 24
local Corner = Instance.new("UICorner")
Corner.CornerRadius = UDim.new(0, 10)
Corner.Parent = ToggleButton
ToggleButton.MouseButton1Click:Connect(function() Window:Minimize() end)

-- [ ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å ]
_G.AutoFarm = false
_G.FastAttack = false
_G.SelectWeapon = "Melee"
_G.FarmMethod = "Above (‡∏ö‡∏ô‡∏´‡∏±‡∏ß)" 
_G.FarmDistance = 10 

-- [ Tabs ]
local Tabs = {
    Main = Window:AddTab({ Title = "General", Icon = "home" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

-- [ ‡∏´‡∏ô‡πâ‡∏≤ General ]
local FarmToggle = Tabs.Main:AddToggle("AutoFarm", {Title = "Auto Farm Level", Default = false })
FarmToggle:OnChanged(function(Value)
    _G.AutoFarm = Value
    _G.FastAttack = Value
end)

Tabs.Main:AddDropdown("FarmMethod", {
    Title = "‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô‡∏ï‡∏µ",
    Values = {"Above (‡∏ö‡∏ô‡∏´‡∏±‡∏ß)", "Below (‡πÉ‡∏ï‡πâ‡∏î‡∏¥‡∏ô)", "Orbit (‡∏ö‡∏¥‡∏ô‡∏ß‡∏ô)", "Fast Warp (‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏£‡∏≠‡∏ö‡∏ï‡∏±‡∏ß)"},
    Default = 1,
    Callback = function(Value) _G.FarmMethod = Value end
})

Tabs.Main:AddSlider("Distance", {
    Title = "‡∏õ‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏¢‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á/‡∏´‡πà‡∏≤‡∏á",
    Default = 10,
    Min = 5,
    Max = 25,
    Rounding = 0,
    Callback = function(Value) _G.FarmDistance = Value end
})

-- [ ‡∏´‡∏ô‡πâ‡∏≤ Settings ]
Tabs.Settings:AddDropdown("Weapon", {
    Title = "Select Weapon",
    Values = {"Melee", "Sword", "Fruit"},
    Default = 1,
    Callback = function(Value) _G.SelectWeapon = Value end
})

-- [[ ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏£‡∏∞‡∏ö‡∏ö‡∏ï‡∏µ (New Fast Attack System) ]]
local VirtualUser = game:GetService("VirtualUser")
task.spawn(function()
    while true do
        task.wait() 
        if _G.FastAttack and _G.AutoFarm then
            pcall(function()
                -- ‡∏™‡πà‡∏á‡∏™‡∏±‡∏ç‡∏ç‡∏≤‡∏ì‡∏ï‡∏µ‡πÅ‡∏ö‡∏ö‡∏ú‡∏™‡∏° (Remotes + VirtualClick)
                local Combat = game:GetService("ReplicatedStorage").Remotes.Combat
                Combat:FireServer()
                VirtualUser:CaptureController()
                VirtualUser:Button1Down(Vector2.new(0,0))
            end)
        end
    end
end)

-- [[ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏¢‡∏¥‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò ]]
function EquipWeapon()
    local lp = game.Players.LocalPlayer
    if lp.Character and not lp.Character:FindFirstChildOfClass("Tool") then
        local tool = lp.Backpack:FindFirstChild(_G.SelectWeapon)
        if tool then lp.Character.Humanoid:EquipTool(tool) end
    end
end

-- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏° Advance ]]
task.spawn(function()
    local Angle = 0
    while task.wait() do
        if _G.AutoFarm then
            pcall(function()
                EquipWeapon()
                local lp = game.Players.LocalPlayer
                local char = lp.Character
                
                if not lp.PlayerGui.Main.Quest.Visible then
                    char.HumanoidRootPart.CFrame = CFrame.new(1059, 15, 1549)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "BanditQuest1", 1)
                else
                    local Enemy = workspace.Enemies:FindFirstChild("Bandit")
                    if Enemy and Enemy.Humanoid.Health > 0 then
                        -- ‡∏ö‡∏ß‡∏Å‡∏•‡∏ö‡∏Ñ‡πà‡∏≤ CFrame ‡∏ï‡∏≤‡∏°‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö
                        local TargetCFrame = Enemy.HumanoidRootPart.CFrame
                        
                        if _G.FarmMethod == "Above (‡∏ö‡∏ô‡∏´‡∏±‡∏ß)" then
                            char.HumanoidRootPart.CFrame = TargetCFrame * CFrame.new(0, _G.FarmDistance, 0)
                        elseif _G.FarmMethod == "Below (‡πÉ‡∏ï‡πâ‡∏î‡∏¥‡∏ô)" then
                            char.HumanoidRootPart.CFrame = TargetCFrame * CFrame.new(0, -_G.FarmDistance, 0)
                        elseif _G.FarmMethod == "Orbit (‡∏ö‡∏¥‡∏ô‡∏ß‡∏ô)" then
                            Angle = Angle + 0.15
                            char.HumanoidRootPart.CFrame = TargetCFrame * CFrame.Angles(0, Angle, 0) * CFrame.new(0, _G.FarmDistance, 5)
                        elseif _G.FarmMethod == "Fast Warp (‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏£‡∏≠‡∏ö‡∏ï‡∏±‡∏ß)" then
                            char.HumanoidRootPart.CFrame = TargetCFrame * CFrame.new(math.random(-7,7), _G.FarmDistance, math.random(-7,7))
                        end
                        
                        -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏´‡∏±‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏≤‡∏®‡∏±‡∏ï‡∏£‡∏π‡∏ï‡∏•‡∏≠‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏µ‡πÇ‡∏î‡∏ô
                        char.HumanoidRootPart.CFrame = CFrame.lookAt(char.HumanoidRootPart.Position, TargetCFrame.Position)
                    else
                        char.HumanoidRootPart.CFrame = CFrame.new(1145, 15, 1630)
                    end
                end
            end)
        end
    end
end)

Fluent:Notify({Title = "U-HUB Fixed!", Content = "‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏£‡∏∞‡∏ö‡∏ö‡∏ï‡∏µ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß‡∏Ñ‡∏£‡∏±‡∏ö", Duration = 5})
