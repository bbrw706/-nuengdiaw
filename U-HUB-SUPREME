    - ‡πÄ‡∏ô‡πâ‡∏ô‡πÄ‡∏™‡∏ñ‡∏µ‡∏¢‡∏£ 100% ‡πÑ‡∏°‡πà‡∏´‡∏•‡∏∏‡∏î ‡πÑ‡∏°‡πà‡πÄ‡∏î‡πâ‡∏á
    - ‡∏£‡∏∞‡∏ö‡∏ö‡∏ö‡∏¥‡∏ô‡∏ü‡∏≤‡∏£‡πå‡∏° (Fly Farm) ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÑ‡∏î‡πâ
    - ‡∏£‡∏∞‡∏ö‡∏ö‡∏ï‡∏µ‡∏£‡∏±‡∏ß‡πÅ‡∏ö‡∏ö Fix Bug (‡∏ï‡∏µ‡πÅ‡∏ô‡πà‡∏ô‡∏≠‡∏ô)
    --------------------------------------------------
]]

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local LP = game.Players.LocalPlayer
local VIM = game:GetService("VirtualInputManager")
local TweenService = game:GetService("TweenService")

-- [ ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á ]
local Window = Fluent:CreateWindow({
    Title = "U-HUB SUPREME üíé",
    SubTitle = "BY Neung (The Best Version)",
    TabWidth = 160, Size = UDim2.fromOffset(580, 460),
    Acrylic = false, Theme = "Dark", MinimizeKey = Enum.KeyCode.U 
})

local Tabs = {
    Main = Window:AddTab({ Title = "AUTO FARM", Icon = "sword" }),
    Teleport = Window:AddTab({ Title = "TELEPORT", Icon = "map" }),
    Settings = Window:AddTab({ Title = "SETTINGS", Icon = "settings" })
}
local Options = Fluent.Options

-- [ ‡πÄ‡∏°‡∏ô‡∏π‡∏ü‡∏≤‡∏£‡πå‡∏° ]
Tabs.Main:AddToggle("AutoFarm", {Title = "‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ó‡∏µ‡πà‡∏î‡∏µ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î", Default = false})
Tabs.Main:AddDropdown("Weapon", {Title = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò", Values = {"Melee", "Sword", "Fruit"}, Default = "Melee"})
Tabs.Main:AddSlider("Height", {Title = "‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏ï‡∏≠‡∏ô‡∏ö‡∏¥‡∏ô‡∏ï‡∏µ", Default = 5, Min = 1, Max = 15, Rounding = 1})

-- [ ‡∏£‡∏∞‡∏ö‡∏ö‡∏ß‡∏≤‡∏£‡πå‡∏õ ]
local function Warp(pos)
    pcall(function()
        LP.Character.HumanoidRootPart.CFrame = pos
    end)
end

if game.PlaceId == 2753915549 then
    Tabs.Teleport:AddButton({Title = "‡πÄ‡∏Å‡∏≤‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô", Callback = function() Warp(CFrame.new(1059, 15, 1545)) end})
    Tabs.Teleport:AddButton({Title = "‡πÄ‡∏Å‡∏≤‡∏∞‡∏•‡∏¥‡∏á", Callback = function() Warp(CFrame.new(-1598, 35, 153)) end})
end

-- [ üî• ‡∏´‡∏±‡∏ß‡πÉ‡∏à‡∏´‡∏•‡∏±‡∏Å: ‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ó‡∏µ‡πà‡∏î‡∏µ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î ]
task.spawn(function()
    while task.wait() do
        if Options.AutoFarm.Value then
            pcall(function()
                local Char = LP.Character
                local Root = Char:FindFirstChild("HumanoidRootPart")
                local Hum = Char:FindFirstChild("Humanoid")
                if not Root or not Hum or Hum.Health <= 0 then return end

                -- ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ (‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå)
                local Target = nil
                local Enemies = workspace:FindFirstChild("Enemies") or workspace
                for _, v in pairs(Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") then
                        Target = v
                        break 
                    end
                end

                if Target then
                    -- 1. ‡∏ñ‡∏∑‡∏≠‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò (‡∏£‡∏∞‡∏ö‡∏ö Smart Equip)
                    local tool = LP.Backpack:FindFirstChild(Options.Weapon.Value) or Char:FindFirstChild(Options.Weapon.Value)
                    if not tool and Options.Weapon.Value == "Melee" then
                        for _, t in pairs(LP.Backpack:GetChildren()) do
                            if t:IsA("Tool") and not t:FindFirstChild("Handle") then tool = t; break end
                        end
                    end
                    if tool and tool.Parent ~= Char then
                        Hum:EquipTool(tool)
                    end

                    -- 2. ‡∏£‡∏∞‡∏ö‡∏ö‡∏ö‡∏¥‡∏ô‡∏ï‡∏µ (Fly Logic)
                    -- ‡∏•‡πá‡∏≠‡∏Ñ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏´‡∏±‡∏ß‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏≤‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏ó‡∏µ‡πà‡∏´‡∏ô‡∏∂‡πà‡∏á‡∏õ‡∏£‡∏±‡∏ö
                    Root.CFrame = Target.HumanoidRootPart.CFrame * CFrame.new(0, Options.Height.Value, 0) * CFrame.Angles(math.rad(-90), 0, 0)

                    -- 3. ‡∏£‡∏∞‡∏ö‡∏ö‡∏ï‡∏µ‡∏£‡∏±‡∏ß (Fast Attack)
                    VIM:SendMouseButtonEvent(0, 0, 0, true, game, 1)
                    VIM:SendMouseButtonEvent(0, 0, 0, false, game, 1)

                    -- 4. ‡∏•‡πá‡∏≠‡∏Ñ‡∏ï‡∏±‡∏ß‡∏Å‡∏±‡∏ô‡∏ï‡∏Å (BodyVelocity)
                    if not Root:FindFirstChild("U_Hold") then
                        local bv = Instance.new("BodyVelocity", Root)
                        bv.Name = "U_Hold"
                        bv.Velocity = Vector3.new(0, 0, 0)
                        bv.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                    end
                else
                    -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏°‡∏≠‡∏ô ‡πÉ‡∏´‡πâ‡∏•‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡πá‡∏≠‡∏Ñ‡∏≠‡∏≠‡∏Å
                    if Root:FindFirstChild("U_Hold") then Root.U_Hold:Destroy() end
                end
            end)
        else
            -- ‡∏õ‡∏¥‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏° ‡∏•‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡πá‡∏≠‡∏Ñ
            pcall(function()
                if LP.Character.HumanoidRootPart:FindFirstChild("U_Hold") then
                    LP.Character.HumanoidRootPart.U_Hold:Destroy()
                end
            end)
        end
    end
end)

-- [ ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏£‡∏±‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à ]
Fluent:Notify({
    Title = "U-HUB V.BEST",
    Content = "‡∏û‡∏µ‡πà‡πÄ‡∏õ‡πá‡∏ô‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏à‡πÉ‡∏´‡πâ‡∏ô‡∏∞‡∏´‡∏ô‡∏∂‡πà‡∏á! ‡∏•‡∏∏‡∏¢‡πÄ‡∏•‡∏¢‡∏ô‡πâ‡∏≠‡∏á‡∏ä‡∏≤‡∏¢!",
    Duration = 10
})
