                    local Enemy = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name ==
-- [[ üìç ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Stats ]]
local StatsSettings = {
    Selected = "Melee", -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    Enabled = false
}

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Stats ]]
Tabs.Misc:AddParagraph({Title = "AUTO STATS", Content = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∞‡∏≠‡∏±‡∏õ‡πÅ‡∏ï‡πâ‡∏°‡∏•‡∏á‡∏ó‡∏µ‡πà‡πÑ‡∏´‡∏ô"})

local StatsDropdown = Tabs.Misc:AddDropdown("StatsSelect", {
    Title = "Select Stats (‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏™‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏à‡∏∞‡∏≠‡∏±‡∏õ)",
    Values = {"Melee", "Defense", "Sword", "Gun", "Demon Fruit"},
    Default = "Melee",
})

StatsDropdown:OnChanged(function(Value)
    StatsSettings.Selected = Value
end)

Tabs.Misc:AddToggle("AutoStatsToggle", {
    Title = "Start Auto Stats",
    Default = false
}):OnChanged(function(Value)
    StatsSettings.Enabled = Value
end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£ Stats ]]
task.spawn(function()
    while task.wait(1) do -- ‡πÄ‡∏ä‡πá‡∏Å‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏û‡∏≠ ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏£‡∏±‡∏ß‡∏°‡∏≤‡∏Å
        if StatsSettings.Enabled then
            pcall(function()
                local Points = game.Players.LocalPlayer.Data.StatsPoints.Value
                if Points > 0 then
                    -- ‡∏™‡πà‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà Server ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡∏™‡πÅ‡∏ï‡∏ó
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", StatsSettings.Selected, Points)
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Equip ]]
_G.AutoEquip = false
_G.WeaponToHold = "" -- ‡∏ä‡∏∑‡πà‡∏≠‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ó‡∏µ‡πà‡∏´‡∏ô‡∏∂‡πà‡∏á‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Equip ]]
Tabs.Misc:AddParagraph({Title = "AUTO EQUIP", Content = "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏à‡∏∞‡∏´‡∏¢‡∏¥‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥"})

Tabs.Misc:AddToggle("AutoEquipToggle", {
    Title = "Auto Equip Weapon",
    Default = false
}):OnChanged(function(v)
    _G.AutoEquip = v
end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£ Equip ]]
task.spawn(function()
    while task.wait(0.5) do
        if _G.AutoEquip then
            pcall(function()
                local Char = game.Players.LocalPlayer.Character
                local Backpack = game.Players.LocalPlayer.Backpack
                
                -- ‡∏ñ‡πâ‡∏≤‡πÉ‡∏ô‡∏°‡∏∑‡∏≠‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò
                if not Char:FindFirstChildOfClass("Tool") then
                    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤
                    for _, tool in pairs(Backpack:GetChildren()) do
                        if tool:IsA("Tool") then
                            -- ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏´‡∏¢‡∏¥‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ä‡∏¥‡πâ‡∏ô‡πÅ‡∏£‡∏Å‡∏ó‡∏µ‡πà‡πÄ‡∏à‡∏≠ (‡∏´‡∏£‡∏∑‡∏≠‡∏ä‡∏¥‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÅ‡∏£‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î)
                            Char.Humanoid:EquipTool(tool)
                        end
                    end
                end
            end)
        end
    end
end)
-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö Anti-AFK : ‡∏£‡∏±‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå ]]
task.spawn(function()
    local VirtualUser = game:GetService("VirtualUser")
    
    -- ‡πÉ‡∏ä‡πâ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏ä‡πá‡∏Ñ‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏Ç‡∏≠‡∏á‡πÄ‡∏Å‡∏°
    game:GetService("Players").LocalPlayer.Idled:Connect(function()
        -- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏°‡∏ï‡∏£‡∏ß‡∏à‡∏û‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏£‡∏≤‡∏≠‡∏¢‡∏π‡πà‡∏ô‡∏¥‡πà‡∏á (Idled)
        -- ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏à‡∏∞‡∏™‡∏±‡πà‡∏á‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏¢‡∏±‡∏ö‡πÄ‡∏°‡πâ‡∏≤‡∏™‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏ö‡∏≤‡πÜ ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏•‡∏≠‡∏Å‡∏£‡∏∞‡∏ö‡∏ö
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
        
        -- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÉ‡∏ô Console ‡∏ß‡πà‡∏≤‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏≥‡∏á‡∏≤‡∏ô (‡πÄ‡∏≠‡∏≤‡πÑ‡∏ß‡πâ‡∏î‡∏π‡∏ß‡πà‡∏≤‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î‡πÑ‡∏î‡πâ‡∏à‡∏£‡∏¥‡∏á‡πÑ‡∏´‡∏°)
        warn("U-HUB: Anti-AFK is working to keep you in game!")
    end)
end)

-- [[ üé® (‡πÅ‡∏ñ‡∏°) ‡∏õ‡∏∏‡πà‡∏°‡∏•‡∏î‡πÅ‡∏•‡∏Ñ‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠‡∏Ç‡∏≤‡∏ß : White Screen ]]
-- ‡∏ä‡πà‡∏ß‡∏¢‡πÉ‡∏´‡πâ‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠‡πÑ‡∏°‡πà‡∏£‡πâ‡∏≠‡∏ô‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Ç‡πâ‡∏≤‡∏°‡∏Ñ‡∏∑‡∏ô
Tabs.Misc:AddToggle("WhiteScreen", {
    Title = "White Screen (‡∏õ‡∏£‡∏∞‡∏´‡∏¢‡∏±‡∏î‡πÅ‡∏ö‡∏ï/‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡πâ‡∏≠‡∏ô)",
    Default = false
}):OnChanged(function(v)
    _G.WhiteScreenEnabled = v
end)

task.spawn(function()
    while task.wait(1) do
        if _G.WhiteScreenEnabled then
            -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏Å‡∏£‡∏≤‡∏ü‡∏¥‡∏Å‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß
            game:GetService("RunService"):Set3dRenderingEnabled(false)
        else
            -- ‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏ï‡∏≤‡∏°‡∏õ‡∏Å‡∏ï‡∏¥
            game:GetService("RunService"):Set3dRenderingEnabled(true)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏ó‡∏µ‡πà 1 ‡πÇ‡∏•‡∏Å 2 ]]
local Sea2_Island1 = {
    Name = "Kingdom of Rose",
    QuestPos = CFrame.new(-426.3, 73.2, 1836.5), -- ‡∏à‡∏∏‡∏î‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™‡∏´‡∏ô‡πâ‡∏≤‡πÄ‡∏°‡∏∑‡∏≠‡∏á
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 1 (Raider) - ‡πÄ‡∏•‡πÄ‡∏ß‡∏• 700
    Mob1 = { Name = "Raider", QuestID = 1, MonsterPos = CFrame.new(-650.4, 73.2, 1750.5) },
    -- ‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 2 (Mercenary) - ‡πÄ‡∏•‡πÄ‡∏ß‡∏• 725
    Mob2 = { Name = "Mercenary", QuestID = 2, MonsterPos = CFrame.new(-1050.2, 73.2, 1620.8) }
}

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÇ‡∏•‡∏Å 2 ]]
Tabs.Main:AddParagraph({Title = "SECOND SEA - ISLAND 1", Content = "‡∏≠‡∏≤‡∏ì‡∏≤‡∏à‡∏±‡∏Å‡∏£‡∏Å‡∏∏‡∏´‡∏•‡∏≤‡∏ö (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 700-750)"})

Tabs.Main:AddToggle("FarmRaider", { Title = "Auto Farm: Raider", Default = false }):OnChanged(function(v) _G.Farm_Raider = v end)
Tabs.Main:AddToggle("FarmMercenary", { Title = "Auto Farm: Mercenary", Default = false }):OnChanged(function(v) _G.Farm_Mercenary = v end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÇ‡∏•‡∏Å 2 ]]
task.spawn(function()
    while task.wait() do
        if _G.Farm_Raider or _G.Farm_Mercenary then
            pcall(function()
                local MyChar = game.Players.LocalPlayer.Character
                local Root = MyChar.HumanoidRootPart
                
                -- ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
                local TargetData = _G.Farm_Raider and Sea2_Island1.Mob1 or Sea2_Island1.Mob2
                
                if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("Quest") then
                    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™ (Area1Quest)
                    Root.CFrame = Sea2_Island1.QuestPos
                    task.wait(0.5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "Area1Quest", TargetData.QuestID)
                else
                    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏à‡πà‡∏≠‡∏´‡∏±‡∏ß
                    local Enemy = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == TargetData.Name and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            Enemy = v
                            break
                        end
                    end

                    if Enemy then
                        Root.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0, 12, 0) -- ‡πÇ‡∏•‡∏Å 2 ‡∏°‡∏≠‡∏ô‡∏ï‡∏±‡∏ß‡πÉ‡∏´‡∏ç‡πà‡∏Ç‡∏∂‡πâ‡∏ô ‡∏û‡∏µ‡πà‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏õ‡πá‡∏ô 12 ‡∏ô‡∏∞
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Attack", {[1] = Enemy.HumanoidRootPart.Position, [2] = {[1] = Enemy}})
                    else
                        Root.CFrame = TargetData.MonsterPos
                    end
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏™‡πà‡∏ß‡∏ô‡∏ï‡πà‡∏≠‡∏Ç‡∏¢‡∏≤‡∏¢‡πÇ‡∏•‡∏Å 2 ]]
local Sea2_Data = {
    -- ‡πÄ‡∏Å‡∏≤‡∏∞‡∏Å‡∏∏‡∏´‡∏•‡∏≤‡∏ö ‡πÇ‡∏ã‡∏ô 2 (Swan Pirates)
    SwanPirate = {
        QuestPos = CFrame.new(-634.4, 73.2, 1475.2),
        Name = "Swan Pirate",
        QuestID = 1,
        MonsterPos = CFrame.new(-1150.5, 73.2, 1380.4)
    },
    -- ‡πÄ‡∏Å‡∏≤‡∏∞ Green Bit (‡πÄ‡∏ñ‡∏≤‡∏ß‡∏±‡∏•‡∏¢‡πå‡∏¢‡∏±‡∏Å‡∏©‡πå)
    GreenBit = {
        QuestPos = CFrame.new(-2440.5, 73.2, -2635.4),
        Name = "Tushita Quest", -- ‡∏ä‡∏∑‡πà‡∏≠‡∏Å‡∏•‡∏∏‡πà‡∏°‡πÄ‡∏Ñ‡∏ß‡∏™
        Mob1 = { Name = "Marine Captain", QuestID = 1, MonsterPos = CFrame.new(-2850.2, 73.2, -3050.4) },
        Mob2 = { Name = "Marine Admiral", QuestID = 2, MonsterPos = CFrame.new(-3200.4, 73.2, -2980.2) } -- ‡∏ö‡∏≠‡∏™‡πÄ‡∏•‡πá‡∏Å
    }
}

-- [[ üé® ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏Å‡∏≤‡∏∞‡∏Å‡∏∏‡∏´‡∏•‡∏≤‡∏ö Zone 2 & Green Bit ]]
Tabs.Main:AddParagraph({Title = "KINGDOM OF ROSE - ZONE 2", Content = "‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏û‡∏ß‡∏Å‡∏™‡∏ß‡∏≠‡∏ô (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 775-850)"})

Tabs.Main:AddToggle("FarmSwanPirate", { Title = "Auto Farm: Swan Pirate", Default = false }):OnChanged(function(v) _G.Farm_SwanPirate = v end)

Tabs.Main:AddParagraph({Title = "GREEN BIT", Content = "‡πÄ‡∏Å‡∏≤‡∏∞‡πÄ‡∏ñ‡∏≤‡∏ß‡∏±‡∏•‡∏¢‡πå‡∏¢‡∏±‡∏Å‡∏©‡πå (‡πÄ‡∏•‡πÄ‡∏ß‡∏• 875-925)"})

Tabs.Main:AddToggle("FarmMarineCaptain", { Title = "Auto Farm: Marine Captain", Default = false }):OnChanged(function(v) _G.Farm_MarineCaptain = v end)

-- [[ üöÄ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏Å‡∏±‡∏î (‡∏£‡∏±‡∏ô‡πÅ‡∏¢‡∏Å‡∏Å‡∏±‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏•‡∏∑‡πà‡∏ô) ]]
task.spawn(function()
    while task.wait() do
        if _G.Farm_SwanPirate or _G.Farm_MarineCaptain then
            pcall(function()
                local MyChar = game.Players.LocalPlayer.Character
                local Root = MyChar.HumanoidRootPart
                
                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
                local Target = nil
                local QName = ""
                local QID = 1
                local QPos = nil
                local MPos = nil

                if _G.Farm_SwanPirate then
                    Target = Sea2_Data.SwanPirate.Name
                    QName = "Area2Quest"
                    QID = Sea2_Data.SwanPirate.QuestID
                    QPos = Sea2_Data.SwanPirate.QuestPos
                    MPos = Sea2_Data.SwanPirate.MonsterPos
                elseif _G.Farm_MarineCaptain then
                    Target = Sea2_Data.GreenBit.Mob1.Name
                    QName = "GreenBitQuest"
                    QID = Sea2_Data.GreenBit.Mob1.QuestID
                    QPos = Sea2_Data.GreenBit.QuestPos
                    MPos = Sea2_Data.GreenBit.Mob1.MonsterPos
                end

                -- ‡∏£‡∏∞‡∏ö‡∏ö‡∏£‡∏±‡∏ö‡πÄ‡∏Ñ‡∏ß‡∏™‡πÅ‡∏•‡∏∞‡∏ï‡∏µ (Logic ‡πÄ‡∏î‡∏¥‡∏°‡∏ó‡∏µ‡πà‡∏´‡∏ô‡∏∂‡πà‡∏á‡∏Ñ‡∏∏‡πâ‡∏ô‡πÄ‡∏Ñ‡∏¢)
                if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("Quest") then
                    Root.CFrame = QPos
                    task.wait(0.5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", QName, QID)
                else
                    local Enemy = nil
                    for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == Target and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            Enemy = v
                            break
                        end
                    end

                    if Enemy then
                        Root.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0, 12, 0)
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Attack", {[1] = Enemy.HumanoidRootPart.Position, [2] = {[1] = Enemy}})
                    else
                        Root.CFrame = MPos
                    end
                end
            end)
        end
    end
end)
-- [[ üìç ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏Å‡∏≤‡∏∞‡∏™‡∏∏‡∏™‡∏≤‡∏ô‡πÅ‡∏•‡∏∞‡πÄ‡∏Å‡∏≤‡∏∞‡∏´‡∏¥‡∏°‡∏∞ ]]
local Sea2_Late_Data = {
    -- ‡πÄ‡∏Å‡∏≤‡∏∞‡∏™‡∏∏‡∏™‡∏≤‡∏ô (Graveyard)
    Graveyard = {
